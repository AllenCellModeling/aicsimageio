  <!-- 4DN-specific calibration elements -->
  <xsd:element name="FIeldCalibration">
    <xsd:annotation>
      <xsd:documentation>Description=This describes the procedure used to measure the field dependent variations in the pixel-to-pixel sensitivity of the detector and/or by distortions in the optical path.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element maxOccurs="unbounded" name="FlatFieldImageRef">
          <xsd:annotation>
            <xsd:documentation>Description=An image describing field dependent variations in the pixel-to-pixel sensitivity of the detector and/or by distortions in the optical path.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference">
                <xsd:attribute name="ID" type="LSID">
                  <xsd:annotation>
                    <xsd:documentation>Tier=3</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:extension>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="0" name="AnnotationRef">
          <xsd:annotation>
            <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference"/>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" type="LSID" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this element.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Date" type="xsd:date" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=The date in which this calibration procedure was performed.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="OpticalCalibration">
    <xsd:annotation>
      <xsd:documentation>Description=This describes the procedure that was used for optical calibration and the resulting calibration measurements.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element maxOccurs="unbounded" minOccurs="1" name="OpticalCalibrationImageRef">
          <xsd:annotation>
            <xsd:documentation>Description=An image (typically a z-stack) obtained by observing the optical standard used in this instance and acquired using the same instrument and settings employed to acquire the image this calibration refers to.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference">
                <xsd:attribute name="ID" type="ImageID" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Tier=3</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:extension>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:element maxOccurs="unbounded" ref="OpticalStandardGroup"/>
        <xsd:element maxOccurs="unbounded" minOccurs="0" name="OpticalCalibrationReportRef">
          <xsd:annotation>
            <xsd:documentation>Description=This element refers to a file containing the report ouput generated by the optical calibration software.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference">
                <xsd:attribute name="ID" type="OpticalCalibrationReportID" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Tier=3</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="OpticalCalibrationSoftware " type="xsd:string">
                  <xsd:annotation>
                    <xsd:documentation>Description=The name of the software tool utilized to generate the OpticalCalibration report. As an example, such a tool might analyzes microscopy images of fluorescent beads in order to determine quantitatively the resolution and chromatic aberrations of a fluorescence microsocope over the full field of view. An example of such a tool is PSFj (http://www.knoplab.de/psfj/sample-page/).</xsd:documentation>
                    <xsd:documentation>Tier=3</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:extension>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="0" name="AnnotationRef">
          <xsd:annotation>
            <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference"/>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" type="LSID" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this element.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Date" type="xsd:date" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=The date in which this calibration was performed.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MedianSpotSize_FWHM" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Observed median lateral spot size expressed as the Full Width at Half Maximum (FWHM) of the fitted Point Spread Function (PSF) intensity profile. This is a measure of the observed resolution of the system in the xy-plane.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute default="nm" name="MedianSpotSize_FWHMUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MedianTheoretical_FWHM" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Theoretical (expected) medians lateral spot size expressed as the Full Width at Half Maximum (FWHM) of the fitted Point Spread Function (PSF) intensity profile, on the basis of the given optical conditions. This is a measure of the expected resolution of the system in the xy-plane.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute default="nm" name="MedianTheoretical_FWHMUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MinSpotSize_FWHM" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Observed minimum lateral spot size expressed as the Full Width at Half Maximum (FWHM) of the fitted Point Spread Function (PSF) intensity profile. This is a measure of the observed resolution of the system in the xy-plane.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute default="nm" name="MinSpotSize_FWHMUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MinTheoretical_FWHM" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Theoretical (expected) minimum lateral spot size expressed as the Full Width at Half Maximum (FWHM) of the fitted Point Spread Function (PSF) intensity profile, on the basis of the given optical conditions. This is a measure of the expected resolution of the system in the xy-plane.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MinTheoretical_FWHMUnit" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Theoretical (expected) minimum lateral spot size expressed as the Full Width at Half Maximum (FWHM) of the fitted Point Spread Function (PSF) intensity profile, on the basis of the given optical conditions. This is a measure of the expected resolution of the system in the xy-plane.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MaxSpotSize_FWHM" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Observed maximum lateral spot size expressed as the Full Width at Half Maximum (FWHM) of the fitted Point Spread Function (PSF) intensity profile. This is a measure of the observed resolution of the system in the xy-plane.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute default="nm" name="MaxSpotSize_FWHMUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MaxTheoretical_FWHM" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Theoretical (expected) maximum lateral spot size expressed as the Full Width at Half Maximum (FWHM) of the fitted Point Spread Function (PSF) intensity profile, on the basis of the given optical conditions. This is a measure of the expected resolution of the system in the xy-plane.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MaxTheoretical_FWHMUnit" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Theoretical (expected) maximum lateral spot size expressed as the Full Width at Half Maximum (FWHM) of the fitted Point Spread Function (PSF) intensity profile, on the basis of the given optical conditions. This is a measure of the expected resolution of the system in the xy-plane.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ZSpotSize_FWHM" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Observed z-axial spot size expressed as the Full Width at Half Maximum (FWHM) of the fitted Point Spread Function (PSF) intensity profile. This is a measure of the observed resolution of the system in the z-plane.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ZSpotSize_FWHMUnit" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Observed z-axial spot size expressed as the Full Width at Half Maximum (FWHM) of the fitted Point Spread Function (PSF) intensity profile. This is a measure of the observed resolution of the system in the z-plane.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="FWHM_ZTheoretical" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Theoretical (expected) z-axial spot size expressed as the Full Width at Half Maximum (FWHM) of the fitted Point Spread Function (PSF) intensity profile, on the basis of the given optical conditions. This is a measure of the expected resolution of the system in the z-plane.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute default="nm" name="FWHM_ZTheoreticalUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Planarity" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=This is a measure of the flatness of the focal plane across the Field of View (FOV). The planarity of the focal plane is calculated from the observed z-position of the best focus of each spot. It indicates focal aberrations or tilt.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute default="nm" name="PlanarityUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Asymmetry" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=This field reports the mean +/- standard deviation over the Field of View (FOV) of the observed Point Spread Function (PSF) Asymmetry (i.e. the ratio of the minimum lateral FWMH over the maximum lateral FWHM). It indicates the presence of optical aberrations.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="LateralChromaticShift" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The lateral offset of the observed spot position between two color channels.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute default="nm" name="LateralChromaticShiftUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="AxialChromaticShift " type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The axial offset of the observed spot position between two color channels.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute default="nm" name="AxialChromaticShiftUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="OpticalStandard">
    <xsd:annotation>
      <xsd:documentation>Description=The optical standard used in this case. This might be a pre-manufactured slide, such as those produced by Argolight (http://argolight.com/), Geller MicroAnalytical Laboratory (Magnification Reference Standard MRS-4 and others; http://www.gellermicro.com/mag_standards/mrs.html), Zeiss and others. Alternatively, the standard might be a slide containing multi-colored beads of known size and emission characteristics. Finally, slides coated with specific DNA origami patterns are increasingly being used for this purpose as well.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ManufacturerSpec">
        <xsd:sequence>
          <xsd:element maxOccurs="unbounded" minOccurs="0" ref="AnnotationRef"/>
        </xsd:sequence>
        <xsd:attribute name="ID" type="LSID" use="required">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
            <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Name" type="Denomination" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element abstract="true" name="OpticalStandardGroup" type="OpticalStandard">
    <xsd:annotation>
      <xsd:documentation>Description=The optical standard used in this case. This might be a pre-manufactured slide, such as those produced by Argolight (http://argolight.com/), Geller MicroAnalytical Laboratory (Magnification Reference Standard MRS-4 and others; http://www.gellermicro.com/mag_standards/mrs.html), Zeiss and others. Alternatively, the standard might be a slide containing multi-colored beads of known size and emission characteristics. Finally, slides coated with specific DNA origami patterns are increasingly being used for this purpose as well.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ColoredBeadsSlide" substitutionGroup="OpticalStandardGroup">
    <xsd:annotation>
      <xsd:documentation>Description=This element describes the beads that were used to acquire the image.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="OpticalStandard">
          <xsd:attribute name="BeadSize" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The size of the colred beads utilized to make this slide.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="nm" name="BeadSizeUnit" type="UnitsLength" use="optional"/>
          <xsd:attribute name="BeadConcentration" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The concentration of the colored beads preparation utilized to make this slide.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="Units/ml" name="BeadConcentrationUnit" type="UnitsConcentration"/>
          <xsd:attribute name="BeadVolume" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The volume of the colored beads preparation utilized to prepare this slide.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="Âµl" name="BeadVolumeUnit" type="UnitsVolume"/>
          <xsd:attribute name="BeadEmissionWavelenght" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The wavelenght(s) of the light emitted by these beads.</xsd:documentation>
              <xsd:documentation>Tier-3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="nm" name="BeadEmissionWavelenghtUnit" type="UnitsLength"/>
          <xsd:attribute name="Type" type="BeadType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The type of colred beads used in this case.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CalibrationSlide" substitutionGroup="OpticalStandardGroup">
    <xsd:annotation>
      <xsd:documentation>Description=This element describes a slide used to measure the different optical properties such as magnification, distance, and different kinds of optical distorsions.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="OpticalStandard">
          <xsd:attribute name="Type" type="CalibrationSlideType">
            <xsd:annotation>
              <xsd:documentation>Description=The type of calibration slide used in this case. Examples include slides produced by Argolight, Geller MicroAnalytical Laboratory, Zeiss and others.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="CalibratedMeasure" type="CalibratedMeasure">
            <xsd:annotation>
              <xsd:documentation>Description=The optical characteristic this slide is used to calibrate, examples include magnification, distance, optical distorsions and others.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DNAOrigami" substitutionGroup="OpticalStandardGroup">
    <xsd:annotation>
      <xsd:documentation>Description=This element describes an optical standard constructed using the DNA origani technique.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="OpticalStandard">
          <xsd:attribute name="Type" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Description=This field describes the type of DNA origami-based optical standard used in this case. Currently this is fild is a simple string. However it might be substituted by an enumeration at a later stage.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="IntensityCalibrationTool">
    <xsd:annotation>
      <xsd:documentation>Category=Calibration</xsd:documentation>
      <xsd:documentation>Description=A tool, such as the Meta-Max calibration toold developed by the 4D Nucleome Imaging Standards Working Group, used to perform an intensity calibration procedure.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ManufacturerSpec">
          <xsd:sequence>
            <xsd:element minOccurs="0" name="AnnotationRef">
              <xsd:annotation>
                <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
                <xsd:documentation>Tier=3</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:complexContent>
                  <xsd:extension base="Reference"/>
                </xsd:complexContent>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="ID" type="LSID" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Name" type="Denomination" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Type" type="IntensityCalibrationType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The type of calibration proceudure this tool is intended to perform.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="IntensityCalibration">
    <xsd:annotation>
      <xsd:documentation>Description=This describes the procedure(s) that were used for excitation and detector calibration and the resulting calibration results.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="IntensityCalibrationToolRef" type="Reference" maxOccurs="unbounded"
        minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Description=This element is a reference to the tool, if any, used to perform this calibratin procdure.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AnnotationRef">
        <xsd:annotation>
          <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="Reference"/>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="ID" type="LSID" use="required">
      <xsd:annotation>
        <xsd:documentation>Description=A unique identifier for this element.</xsd:documentation>
        <xsd:documentation>Tier=3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Date" type="xsd:date" use="required">
      <xsd:annotation>
        <xsd:documentation>Description=The date in which this calibration procedure was performed.</xsd:documentation>
        <xsd:documentation>Tier=3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ObservedExcitationWavelenght" type="xsd:float">
      <xsd:annotation>
        <xsd:documentation>Description=Observed peak excitation wavelength.</xsd:documentation>
        <xsd:documentation>Tier=3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ObservedExcitationWavelenghtUnit" type="UnitsLength" default="nm">
      <xsd:annotation>
        <xsd:documentation>Tier=3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ObservedExcitationPowerAtObjective" type="xsd:float">
      <xsd:annotation>
        <xsd:documentation>Description=The observed excitation light power measured at the back of the objective.</xsd:documentation>
        <xsd:documentation>Tier=3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ObservedExcitationPowerAtObjectiveUnit" type="UnitsPower" default="W">
      <xsd:annotation>
        <xsd:documentation>Tier=3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ObservedExcitationIrradiance" type="xsd:float">
      <xsd:annotation>
        <xsd:documentation>Description=Observed mean excitation light irradiance (radiant power per unit area) as measured at the sample plane over the Field of View (FOV). </xsd:documentation>
        <xsd:documentation>Tier=3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="W" name="ObservedExcitationIrradianceUnit" type="UnitsPower">
      <xsd:annotation>
        <xsd:documentation>Tier=3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ObservedExcitationVariance" type="xsd:float">
      <xsd:annotation>
        <xsd:documentation>Description=Observed excitation light irradiance (radiant power per unit area) variation over the Field of View (FOV).</xsd:documentation>
        <xsd:documentation>Tier=3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element abstract="true" name="IntensityCalibrationGroup" type="IntensityCalibration">
    <xsd:annotation>
      <xsd:documentation>Description=This describes the procedure(s) that were used for excitation and detector calibration and the resulting calibration results.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>

  <xsd:element name="ExcitationCalibration" substitutionGroup="IntensityCalibrationGroup">
    <xsd:annotation>
      <xsd:documentation>Description=This procedure measures the performance and allows calibration of the wavelength and irradiance of the excitation light.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="IntensityCalibration"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="DetectorCalibration" substitutionGroup="IntensityCalibrationGroup">
    <xsd:annotation>
      <xsd:documentation>Description=This procedure measures the performance of the dectector.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="IntensityCalibration"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CalibrationMap">
    <xsd:annotation>
      <xsd:documentation>Description=This is a 2D matrix containing values computed as a resulut of the calibration procedure.  This matrix is the same size of the image this calibration refers to.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Row" type="rowType" maxOccurs="unbounded">
          <xsd:unique name="cellNrKey">
            <xsd:selector xpath="Cell"/>
            <xsd:field xpath="@cellNr"/>
          </xsd:unique>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="MetricType" type="MatrixTypeEnum">
        <xsd:annotation>
          <xsd:documentation>Description=This field captures the type of metric that was computed as part of the calibratin procedure in this instance.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="IlluminationIntensity" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=This attribute only applies to matrices of type Variance and Mean. It represents the illumination intensity that was used to acquire the images used to contruct this matrix. The value can range from 0 (i.e. dark image) to any intenisty value experssed either as percentage or as absolute power. </xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="IlluminationIntensityUnit"/>
      <xsd:attribute name="NrOfImages" type="xsd:int">
        <xsd:annotation>
          <xsd:documentation>Description=This field represents the number of images that were employed to generate this calibration matrix.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
    <xsd:unique name="rowNrKey">
      <xsd:selector xpath="Row"/>
      <xsd:field xpath="@rowNr"/>
    </xsd:unique>
  </xsd:element>
  <xsd:simpleType name="CalibrationSlideType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ARGO-HM"/>
      <xsd:enumeration value="ARGO-LM"/>
      <xsd:enumeration value="ARGO-SIM"/>
      <xsd:enumeration value="Geller-MRS4"/>
      <xsd:enumeration value="Zeiss universal D=0"/>
      <xsd:enumeration value="Other"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CalibratedMeasure">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Magnification"/>
      <xsd:enumeration value="Distance"/>
      <xsd:enumeration value="Multi"/>
      <xsd:enumeration value="Distorsion"/>
      <xsd:enumeration value="Other"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BeadType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Single color"/>
      <xsd:enumeration value="Multi-color"/>
      <xsd:enumeration value="Other"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="cellType">
    <xsd:attribute name="CellNr" type="xsd:int" use="required"/>
    <xsd:attribute name="CellValue" type="xsd:float"/>
    <xsd:attribute name="CellValueUnit"/>
  </xsd:complexType>
  <xsd:complexType name="rowType">
    <xsd:sequence>
      <xsd:element name="Cell" type="cellType" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="RowNr" type="xsd:int" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="MatrixTypeEnum">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Gain"/>
      <xsd:enumeration value="LinearOffset"/>
      <xsd:enumeration value="DetectionLimit"/>
      <xsd:enumeration value="LinearRangeMax"/>
      <xsd:enumeration value="Variance"/>
      <xsd:enumeration value="Mean"/>
      <xsd:enumeration value="Other"/>
      <xsd:enumeration value=""/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="MechanicalCalibration">
    <xsd:annotation>
      <xsd:documentation>Description=This describes the procedures that were used for mechanical calibration and the resulting calibration results.</xsd:documentation>
      <xsd:documentation>Tier=4</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" ref="SamplePositioningRef"/>
        <xsd:element minOccurs="0" ref="AutoFocusRef"/>
        <xsd:element minOccurs="0" name="AnnotationRef">
          <xsd:annotation>
            <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
            <xsd:documentation>Tier=4</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference"/>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" type="LSID" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this element.</xsd:documentation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Date" type="xsd:date" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=The date in which this calibration was performed.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MeasuredLateralDrift" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=Measured shift in (x, y) position over time.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MeasuredLateralDriftUnit" type="UnitsLength"/>
      <xsd:attribute name="MeasuredFocalDrift">
        <xsd:annotation>
          <xsd:documentation>Description=Measured shift in z position over time.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MeasuredFocalDriftUnit" type="UnitsLength"/>
      <xsd:attribute name="MeasuredRepeatability">
        <xsd:annotation>
          <xsd:documentation>Description=Measured precision with which this device is capable of returning to a previously visited position (useful for tiling). Typically expressed in % error.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MeasuredSettlingTime">
        <xsd:annotation>
          <xsd:documentation>Description=Measured time it takes for this device to stabilize at the target position.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MeasuredSettlingTimeUnit"/>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
