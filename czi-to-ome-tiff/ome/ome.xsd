<xsd:schema xmlns="http://www.openmicroscopy.org/Schemas/OME/2016-06"
  targetNamespace="http://www.openmicroscopy.org/Schemas/OME/2016-06"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:OME="http://www.openmicroscopy.org/Schemas/OME/2016-06"
  xmlns:xml="http://www.w3.org/XML/1998/namespace" version="3" elementFormDefault="qualified">

  <xsd:import namespace="http://www.w3.org/XML/1998/namespace"
    schemaLocation="http://www.w3.org/2001/xml.xsd"/>

  <xsd:annotation>
    <xsd:documentation>
      Open Microscopy Environment
      OME XML Schema June 2016
    </xsd:documentation>
  </xsd:annotation>

  <!-- Top Level OME elements -->
  <xsd:element name="OME">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>
        The OME element is a container for all information objects accessible by OME.
        These information objects include descriptions of the imaging experiments
        and the people who perform them, descriptions of the microscope, the resulting
        images and how they were acquired, the analyses performed on those images,
        and the analysis results themselves.
        An OME file may contain any or all of this information.

        With the creation of the Metadata Only Companion OME-XML and Binary Only OME-TIFF files
        the top level OME node has changed slightly.
        It can EITHER:
        Contain all the previously expected elements
        OR:
        Contain a single BinaryOnly element that points at
        its Metadata Only Companion OME-XML file.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="Rights" minOccurs="0" maxOccurs="1"/>
        <xsd:choice>
          <!-- Either: standard expected elements -->
          <xsd:sequence>
            <xsd:element ref="Project" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="Dataset" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="Folder" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="Experiment" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="Plate" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="Screen" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="Experimenter" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="ExperimenterGroup" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="Instrument" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="Image" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="StructuredAnnotations" minOccurs="0" maxOccurs="1"/>
            <xsd:element ref="ROI" minOccurs="0" maxOccurs="unbounded"/>
          </xsd:sequence>
          <!-- Or: Single BinaryOnly element -->
          <xsd:element name="BinaryOnly" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
              <xsd:documentation> Pointer to an external metadata file. If this
              element is present, then no other metadata may be present in this
              file, i.e. this file is a place-holder. </xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:attribute name="MetadataFile" type="xsd:string" use="required">
                <xsd:annotation>
                  <xsd:documentation> Filename of the OME-XML metadata file for
                  this binary data. If the file cannot be found, a search can
                  be performed based on the UUID. </xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
              <xsd:attribute name="UUID" type="UniversallyUniqueIdentifier" use="required">
                <xsd:annotation>
                  <xsd:documentation> The unique identifier of another OME-XML
                  block whose metadata describes the binary data in this file.
                  This UUID is considered authoritative regardless of
                  mismatches in the filename. </xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
            </xsd:complexType>
          </xsd:element>
          <!-- End of MetadataOnly Element -->
        </xsd:choice>
      </xsd:sequence>
      <xsd:attribute name="UUID" type="UniversallyUniqueIdentifier" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            This unique identifier is used to keep track of multi part files.
            It allows the links between files to survive renaming.
            While OPTIONAL in the general case this is REQUIRED in a
            MetadataOnly Companion to a collection of BinaryOnly files.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            This is the name of the creating application of the OME-XML
            and preferably its full version. e.g "CompanyName, SoftwareName, V2.6.3456"
            This is optional but we hope it will be set by applications writing out OME-XML from scratch.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>

    <!-- Unique IDs -->
    <xsd:key name="ChannelIDKey">
      <xsd:selector xpath="OME:Image/OME:Pixels/OME:Channel"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="DatasetIDKey">
      <xsd:selector xpath="OME:Dataset"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="DetectorIDKey">
      <xsd:selector xpath="OME:Instrument/OME:Detector"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="DichroicIDKey">
      <xsd:selector xpath="OME:Instrument/OME:Dichroic"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ExperimentIDKey">
      <xsd:selector xpath="OME:Experiment"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ExperimenterIDKey">
      <xsd:selector xpath="OME:Experimenter"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="FilterIDKey">
      <xsd:selector xpath="OME:Instrument/OME:Filter"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="FilterSetIDKey">
      <xsd:selector xpath="OME:Instrument/OME:FilterSet"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="FolderIDKey">
      <xsd:selector xpath="OME:Folder"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ExperimenterGroupIDKey">
      <xsd:selector xpath="OME:ExperimenterGroup"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ImageIDKey">
      <xsd:selector xpath="OME:Image"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="InstrumentIDKey">
      <xsd:selector xpath="OME:Instrument"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="LightSourceIDKey">
      <xsd:selector
        xpath="OME:Instrument/OME:Laser | OME:Instrument/OME:Arc | OME:Instrument/OME:Filament | OME:Instrument/OME:LightEmittingDiode | OME:Instrument/OME:GenericExcitationSource"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="MicrobeamManipulationIDKey">
      <xsd:selector xpath="OME:Experiment/OME:MicrobeamManipulation"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ObjectiveIDKey">
      <xsd:selector xpath="OME:Instrument/OME:Objective"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="PixelsIDKey">
      <xsd:selector xpath="OME:Image/OME:Pixels"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="PlateIDKey">
      <xsd:selector xpath="OME:Plate"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ProjectIDKey">
      <xsd:selector xpath="OME:Project"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ReagentIDKey">
      <xsd:selector xpath="OME:Screen/OME:Reagent"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ScreenAcquisitionIDKey">
      <xsd:selector xpath="OME:Screen/OME:ScreenAcquisition"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ScreenIDKey">
      <xsd:selector xpath="OME:Screen"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="WellIDKey">
      <xsd:selector xpath="OME:Plate/OME:Well"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="WellSampleIDKey">
      <xsd:selector xpath="OME:Plate/OME:Well/OME:WellSample"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ROIIDKey">
      <xsd:selector xpath="OME:ROI"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ShapeIDKey">
      <xsd:selector
        xpath="OME:ROI/OME:Union/OME:Rectangle | OME:ROI/OME:Union/OME:Mask | OME:ROI/OME:Union/OME:Ellipse | OME:ROI/OME:Union/OME:Point | OME:ROI/OME:Union/OME:Line | OME:ROI/OME:Union/OME:Polyline | OME:ROI/OME:Union/OME:Polygon | OME:ROI/OME:Union/OME:Label"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="AnnotationIDKey">
      <xsd:selector xpath="OME:StructuredAnnotations/*"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>

    <!-- Unique references -->
    <xsd:key name="WellSampleImageRefIDKey">
      <xsd:selector xpath="OME:Plate/OME:Well/OME:WellSample/OME:ImageRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>

    <!-- Valid references -->
    <xsd:keyref name="DatasetExperimenterIDKeyRef" refer="OME:ExperimenterIDKey">
      <xsd:selector xpath="OME:Dataset/OME:ExperimenterRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="DatasetExperimenterGroupIDKeyRef" refer="OME:ExperimenterGroupIDKey">
      <xsd:selector xpath="OME:Dataset/OME:ExperimenterGroupRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="DatasetProjectIDKeyRef" refer="OME:ProjectIDKey">
      <xsd:selector xpath="OME:Dataset/OME:ProjectRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ExperimentExperimenterIDKeyRef" refer="OME:ExperimenterIDKey">
      <xsd:selector xpath="OME:Experiment/OME:ExperimenterRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ExperimentMicrobeamManipulationExperimenterIDKeyRef"
      refer="OME:ExperimenterIDKey">
      <xsd:selector xpath="OME:Experiment/OME:MicrobeamManipulation/OME:ExperimenterRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ExperimentMicrobeamManipulationLightSourceSettingsLightSourceIDKeyRef"
      refer="OME:LightSourceIDKey">
      <xsd:selector xpath="OME:Experiment/OME:MicrobeamManipulation/OME:LightSourceSettings"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ExperimenterExperimenterGroupIDKeyRef" refer="OME:ExperimenterGroupIDKey">
      <xsd:selector xpath="OME:Experimenter/OME:ExperimenterGroupRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ExperimenterGroupContactExperimenterIDKeyRef" refer="OME:ExperimenterIDKey">
      <xsd:selector xpath="OME:ExperimenterGroup/OME:Contact"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ExperimenterGroupLeaderExperimenterIDKeyRef" refer="OME:ExperimenterIDKey">
      <xsd:selector xpath="OME:ExperimenterGroup/OME:Leader"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="FolderFolderIDKeyRef" refer="OME:FolderIDKey">
      <xsd:selector xpath="OME:Folder/OME:FolderRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="FolderImageIDKeyRef" refer="OME:ImageIDKey">
      <xsd:selector xpath="OME:Folder/OME:ImageRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="FolderROIIDKeyRef" refer="OME:ROIIDKey">
      <xsd:selector xpath="OME:Folder/OME:ROIRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImageDatasetIDKeyRef" refer="OME:DatasetIDKey">
      <xsd:selector xpath="OME:Image/OME:DatasetRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImageExperimenterIDKeyRef" refer="OME:ExperimenterIDKey">
      <xsd:selector xpath="OME:Image/OME:ExperimenterRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImageExperimentIDKeyRef" refer="OME:ExperimentIDKey">
      <xsd:selector xpath="OME:Image/OME:ExperimentRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImageExperimenterGroupIDKeyRef" refer="OME:ExperimenterGroupIDKey">
      <xsd:selector xpath="OME:Image/OME:ExperimenterGroupRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImageInstrumentIDKeyRef" refer="OME:InstrumentIDKey">
      <xsd:selector xpath="OME:Image/OME:InstrumentRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImageMicrobeamManipulationIDKeyRef" refer="OME:MicrobeamManipulationIDKey">
      <xsd:selector xpath="OME:Image/OME:MicrobeamManipulationRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImageObjectiveSettingsObjectiveIDKeyRef" refer="OME:ObjectiveIDKey">
      <xsd:selector xpath="OME:Image/OME:ObjectiveSettings"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImagePixelsChannelDetectorSettingsDetectorIDKeyRef" refer="OME:DetectorIDKey">
      <xsd:selector xpath="OME:Image/OME:Pixels/OME:Channel/OME:DetectorSettings"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImagePixelsChannelFilterSetIDKeyRef" refer="OME:FilterSetIDKey">
      <xsd:selector xpath="OME:Image/OME:Pixels/OME:Channel/OME:FilterSetRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImagePixelsChannelLightPathExcitationFilterIDKeyRef" refer="OME:FilterIDKey">
      <xsd:selector
        xpath="OME:Image/OME:Pixels/OME:Channel/OME:LightPath/OME:ExcitationFilterPath/OME:FilterRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImagePixelsChannelLightPathExcitationDichroicIDKeyRef"
      refer="OME:DichroicIDKey">
      <xsd:selector
        xpath="OME:Image/OME:Pixels/OME:Channel/OME:LightPath/OME:ExcitationFilterPath/OME:DichroicRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImagePixelsChannelLightPathDichroicIDKeyRef" refer="OME:DichroicIDKey">
      <xsd:selector xpath="OME:Image/OME:Pixels/OME:Channel/OME:LightPath/OME:DichroicRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImagePixelsChannelLightPathEmissionFilterIDKeyRef" refer="OME:FilterIDKey">
      <xsd:selector
        xpath="OME:Image/OME:Pixels/OME:Channel/OME:LightPath/OME:EmissionFilterPath/OME:FilterRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImagePixelsChannelLightPathEmissionDichroicIDKeyRef" refer="OME:DichroicIDKey">
      <xsd:selector
        xpath="OME:Image/OME:Pixels/OME:Channel/OME:LightPath/OME:EmissionFilterPath/OME:DichroicRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImagePixelsChannelLightSourceSettingsLightSourceIDKeyRef"
      refer="OME:LightSourceIDKey">
      <xsd:selector xpath="OME:Image/OME:Pixels/OME:Channel/OME:LightSourceSettings"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="InstrumentFilterSetExcitationFilterIDKeyRef" refer="OME:FilterIDKey">
      <xsd:selector xpath="OME:Instrument/OME:FilterSet/OME:ExcitationFilterRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="InstrumentFilterSetDichroicIDKeyRef" refer="OME:DichroicIDKey">
      <xsd:selector xpath="OME:Instrument/OME:FilterSet/OME:DichroicRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="InstrumentFilterSetEmissionFilterIDKeyRef" refer="OME:FilterIDKey">
      <xsd:selector xpath="OME:Instrument/OME:FilterSet/OME:EmissionFilterRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="InstrumentLightSourceLaserPumpLightSourceIDKey" refer="OME:LightSourceIDKey">
      <xsd:selector xpath="OME:Instrument/OME:LightSource/OME:Laser/OME:Pump"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="PlateScreenIDKeyRef" refer="OME:ScreenIDKey">
      <xsd:selector xpath="OME:Plate/OME:ScreenRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="PlateWellReagentIDKeyRef" refer="OME:ReagentIDKey">
      <xsd:selector xpath="OME:Plate/OME:Well/OME:ReagentRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="PlateWellWellSampleImageIDKeyRef" refer="OME:ImageIDKey">
      <xsd:selector xpath="OME:Plate/OME:Well/OME:WellSample/OME:ImageRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ProjectExperimenterIDKeyRef" refer="OME:ExperimenterIDKey">
      <xsd:selector xpath="OME:Project/OME:ExperimenterRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ProjectExperimenterGroupIDKeyRef" refer="OME:ExperimenterGroupIDKey">
      <xsd:selector xpath="OME:Project/OME:ExperimenterGroupRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ScreenPlateIDKeyRef" refer="OME:PlateIDKey">
      <xsd:selector xpath="OME:Screen/OME:PlateRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ScreenScreenAcquisitionWellSampleIDKeyRef" refer="OME:WellSampleIDKey">
      <xsd:selector xpath="OME:Screen/OME:ScreenAcquisition/OME:WellSampleRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ExperimentMicrobeamManipulationROIIDKeyRef" refer="OME:ROIIDKey">
      <xsd:selector xpath="OME:Experiment/OME:MicrobeamManipulation/ROIRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="ImageROIIDKeyRef" refer="OME:ROIIDKey">
      <xsd:selector xpath="OME:Image/ROIRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="AnnotationRefAnnotationIDKeyRef" refer="OME:AnnotationIDKey">
      <xsd:selector xpath=".//OME:AnnotationRef"/>
      <xsd:field xpath="@ID"/>
    </xsd:keyref>
    <xsd:keyref name="AnnotationAnnotatorExperimenterIDKeyRef" refer="OME:ExperimenterIDKey">
      <xsd:selector xpath="OME:StructuredAnnotations/*"/>
      <xsd:field xpath="@Annotator"/>
    </xsd:keyref>

  </xsd:element>
  <xsd:element name="Experimenter">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Experimenters</plural><global/></xsdfu></xsd:appinfo>
      <xsd:documentation>
        This element describes a person who performed an imaging experiment.
        This person may also be a user of the OME system, in which case the UserName element contains their login name.
        Experimenters may belong to one or more groups which are specified using one or more ExperimenterGroupRef elements.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="ExperimenterID"/>
      <xsd:attribute name="FirstName" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>First name, sometime called christian name or given name or forename. [plain text string]</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MiddleName" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Any other names. [plain text string]</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="LastName" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>A person's last name sometimes called surname or family name. [plain text string]</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Email" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>A person's email address. [valid email address as string]</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Institution" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            A person's Institution
            The organizing structure that people belong to other than groups.  A university, or company, etc.
            We do not specify a department element, and do not mean for Institution to be used in this way.
            We simply wish to say XXX at YYY.  Where YYY has a better chance of being tied to a geographically fixed location
            and of being more recognizable than a group of experimenters. [plain text string]
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="UserName" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:appinfo><xsdfu xmlns=""><unique/></xsdfu></xsd:appinfo>
          <xsd:documentation>This is the username of the experimenter (in a 'unix' or 'database' sense). [plain text string]</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ExperimenterGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>ExperimenterGroups</plural><global/></xsdfu></xsd:appinfo>
      <xsd:documentation>
        The ExperimenterGroupID is required.
        Information should ideally be specified for at least one Leader as a contact for the group.
        The Leaders are themselves Experimenters.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>
              A description for the group. [plain-text multi-line string]
            </xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element ref="ExperimenterRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/><childordered/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="Leader" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="Name" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:appinfo><xsdfu xmlns=""><unique/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ID" use="required" type="ExperimenterGroupID"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Project">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Projects</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        The Project ID is required.
        Datasets can be grouped into projects using a many-to-many relationship.
        A Dataset may belong to one or more Projects by including one or more ProjectRef elements which refer to Project IDs.
        Projects do not directly contain images - only by virtue of containing datasets, which themselves contain images.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>
              A description for the project. [plain-text multi-line string]
            </xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element ref="ExperimenterRef" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="ExperimenterGroupRef" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="DatasetRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="Name" use="optional" type="xsd:string"/>
      <xsd:attribute name="ID" use="required" type="ProjectID"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Dataset">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Datasets</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        An element specifying a collection of images that are always processed together.
        Images can belong to more than one Dataset, and a Dataset may contain more than one Image.
        Images contain one or more DatasetRef elements to specify what datasets they belong to.
        Once a Dataset has been processed in any way, its collection of images cannot be altered.
        The ExperimenterRef and ExperimenterGroupRef elements specify the person and group this Dataset belongs to.
        Projects may contain one or more Datasets, and Datasets may belong to one or more Projects.
        This relationship is specified by listing DatasetRef elements within the Project element.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>
              A description for the dataset. [plain-text multi-line string]
            </xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element ref="ExperimenterRef" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="ExperimenterGroupRef" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="ImageRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="Name" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            A name for the dataset that is suitable for presentation to the user.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ID" use="required" type="DatasetID"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Screen">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Screens</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        The Screen element is a grouping for Plates.
        The required attribute is the Screen's Name and ID - both must be unique within the document.
        The Screen element may contain an ExternalRef attribute that refers to an external database.
        A description of the screen may be specified in the Description element.
        Screens may contain overlapping sets of Plates i.e.      Screens and Plates have a many-to-many relationship.
        Plates contain one or more ScreenRef elements to specify what screens they belong to.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>
              A description for the screen.
            </xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element ref="Reagent" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="PlateRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
            <xsd:documentation>
              The PlateRef element is a reference to a Plate element. Screen elements may have one or more PlateRef elements to define the plates that are part of the screen.
              Plates may belong to more than one screen.
            </xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference">
                <xsd:attribute name="ID" use="required" type="PlateID"/>
              </xsd:extension>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="ScreenID"/>
      <xsd:attribute name="Name" use="optional" type="xsd:string"/>
      <xsd:attribute name="ProtocolIdentifier" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            A pointer to an externally defined protocol, usually in a screening database.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ProtocolDescription" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            A description of the screen protocol; may contain very detailed information to
            reproduce some of that found in a screening database.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ReagentSetDescription" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>A description of the set of reagents; may contain very detailed information to reproduce some of that information found in a screening database.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ReagentSetIdentifier" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>A pointer to an externally defined set of reagents, usually in a screeningdatabase/automation database.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Type" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>A human readable identifier for the screen type; e.g. RNAi, cDNA, SiRNA, etc. This string is likely to become an enumeration in future releases.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Plate">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Plates</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>This element identifies microtiter plates within a screen. A plate can belong to more than one screen. The Screen(s) that a plate belongs to are specified by the ScreenRef element. The Plate ID and Name attributes are required. The Wells in a plate are numbers from the top-left corner in a grid starting at zero. i.e. The top-left well of a plate is index (0,0).</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>
              A description for the plate.
            </xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element ref="Well" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="PlateAcquisition" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="PlateID"/>
      <xsd:attribute name="Name" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The Name identifies the plate to the user.
            It is used much like the ID, and so must be
            unique within the document.
            If a plate name is not available when one is needed
            it will be constructed in the following order:
            1. If name is available use it.
            2. If not use "Start time - End time"
            (NOTE: Not a subtraction! A string representation
            of the two times separated by a dash.)
            3. If these times are not available use the Plate ID.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Status" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            A textual annotation of the current state of the plate with respect to the
            experiment work-flow; e.g.
            1. Seed cell: done; 2. Transfection: done;      3. Gel doc: todo.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ExternalIdentifier" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The ExternalIdentifier attribute may contain a reference to an external database.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ColumnNamingConvention" use="optional" type="NamingConvention">
        <xsd:annotation>
          <xsd:documentation>
            The ColumnNamingConvention
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="RowNamingConvention" use="optional" type="NamingConvention">
        <xsd:annotation>
          <xsd:documentation>
            The RowNamingConvention
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="WellOriginX" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>
            This defines the X position to use for the origin of the
            fields (individual images) taken in a well. It is used
            with the X in the WellSample to display the fields
            in the correct position relative to each other. Each Well
            in the plate has the same well origin. Units are set by WellOriginXUnit.

            In the OMERO clients by convention we display the WellOrigin
            in the center of the view.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="WellOriginXUnit" use="optional" default="reference frame"
        type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>The units of the well origin in X - default:reference frame.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="WellOriginY" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>
            This defines the Y position to use for the origin of the
            fields (individual images) taken in a well. It is used
            with the Y in the WellSample to display the fields
            in the correct position relative to each other. Each Well
            in the plate has the same well origin.  Units are set by WellOriginYUnit.

            In the OMERO clients by convention we display the WellOrigin
            in the center of the view.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="WellOriginYUnit" use="optional" default="reference frame"
        type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>The units of the well origin in Y - default:reference frame.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Rows" use="optional" type="PositiveInt">
        <xsd:annotation>
          <xsd:documentation>
            The number of rows in the plate
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Columns" use="optional" type="PositiveInt">
        <xsd:annotation>
          <xsd:documentation>
            The number of columns in the plate
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="FieldIndex" use="optional" type="NonNegativeInt">
        <xsd:annotation>
          <xsd:documentation>
            The index of the WellSample to display as the default Field
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
    <!-- Unique Indexes -->
    <xsd:key name="WellSampleIndex">
      <xsd:selector xpath="Well/WellSample"/>
      <xsd:field xpath="@Index"/>
    </xsd:key>

  </xsd:element>
  <xsd:element name="Folder">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Folders</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        An element specifying a possibly heterogeneous collection of data.
        Folders may contain Folders so that data may be organized within a tree of Folders.
        Data may be in multiple Folders but a Folder may not be in more than one other Folder.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>
              A description for the folder. [plain-text multi-line string]
            </xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element ref="FolderRef" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ImageRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="ROIRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="FolderID"/>
      <xsd:attribute name="Name" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            A name for the folder that is suitable for presentation to the user.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ROI">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>ROIs</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        A four dimensional 'Region of Interest'.
        If they are not used, and the Image has more than one plane,
        the entire set of planes is assumed to be included in the ROI.
        Multiple ROIs may be specified.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:choice minOccurs="1" maxOccurs="1">
          <xsd:element name="Union" minOccurs="1" maxOccurs="1">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element ref="ShapeGroup" minOccurs="1" maxOccurs="unbounded"/>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:choice>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>
              A description for the ROI. [plain-text multi-line string]
            </xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="ROIID"/>
      <xsd:attribute name="Name" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The Name identifies the ROI to the user. [plain-text string]
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Experiment">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo>This element describes the type of experiment. The required Type attribute must contain one or more entries from the following list: FRET, Time-lapse, 4D+, HightThroughputScreen, FISH, Electrophysiology, Ion-Imaging, Co-localization, PGI/Documentation FRAP, Photo-ablation, Optical-Trapping, Photo-activation, Fluorescence-Lifetime, Spectral-Imaging, Other The required LabellingMethod attribute must contain one or more entries from the following list: FluorescentProtein, CASFISH, FISH, Single-Molecule-FISH, Indirect-Immunofluorescene, Immunocytochemistry, None, Other PGI/Documentation is not a 'data' image. The optional Description element may contain free text to further describe the experiment</xsd:appinfo>
      <xsd:documentation>This element describes the type of experiment.
        The required Type attribute must contain one or more entries from the following list:
        FRET, Time-lapse, 4D+, HightThroughputScreen, FISH, Electrophysiology, Ion-Imaging, Co-localization, PGI/Documentation FRAP, Photo-ablation, Optical-Trapping, Photo-activation, Fluorescence-Lifetime, Spectral-Imaging, Other
        The required LabellingMethod attribute must contain one or more entries from the following list:
        FluorescentProtein, CASFISH, FISH, Single-Molecule-FISH, Indirect-Immunofluorescene, Immunocytochemistry, None, Other
        PGI/Documentation is not a 'data' image.
        The optional Description element may contain free text to further describe the experiment.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ExperimenterRef" minOccurs="1" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element has a required reference (the ExperimenterID attribute) to a predefined Experimenter.
            </xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element maxOccurs="1" ref="Sample"/>
        <xsd:element ref="MicrobeamManipulation" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>Description=A description for this imaging Experiment. [plain-text multi-line string]
            </xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element minOccurs="0" ref="AnnotationRef">
          <xsd:annotation>
            <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="ExperimentID">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this Experiment.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Name" type="Denomination" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=A user assigned name for this Experiment.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Type" use="optional" type="ExperimentType">
        <xsd:annotation>
          <xsd:documentation>Description=A term to describe the type of experiment.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="LabellingMethod" type="ExperimentLabellingMethod">
        <xsd:annotation>
          <xsd:documentation>Description=A term that describes the method used for labelling the molecular component under study in this experiment</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Image">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Images</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>This element describes the actual image and its meta-data. The elements that are references (ending in Ref or Settings) refer to elements defined outside of the Image element. Ref elements are simple links, while Settings elements are links with additional values. If any of the required Image attributes or elements are missing, its guaranteed to be an invalid document. The required attributes and elements are ID and Pixels. ExperimenterRef is required for all Images with well formed LSIDs. ImageType is a vendor-specific designation of the type of image this is. Examples of ImageType include 'STK', 'SoftWorx', etc. The Name attributes are in all cases the name of the element instance. In this case, the name of the image, not necessarily the filename. Physical size of pixels are microns[Âµm].
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="InstrumentRef" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="ExperimentRef" minOccurs="1" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>Description=This element describes the type of experiment. The required Type attribute must contain one or more entries from the following list: FRET, Time-lapse, 4D+, HightThroughputScreen, FISH, Electrophysiology, Ion-Imaging, Co-localization, PGI/Documentation FRAP, Photo-ablation, Optical-Trapping, Photo-activation, Fluorescence-Lifetime, Spectral-Imaging, Other The required LabellingMethod attribute must contain one or more entries from the following list: FluorescentProtein, CASFISH, FISH, Single-Molecule-FISH, Indirect-Immunofluorescene, Immunocytochemistry, None, Other PGI/Documentation is not a 'data' image. The optional Description element may contain free text to further describe the experiment</xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="ExperimenterRef" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ExperimenterGroupRef" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="MicrobeamManipulationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
            <xsd:documentation>Description=This empty element has a reference (the MicroBeamManipulationID attribute) to a predefined Microbeam Manipulation (e.g. a microbeam operation, such as a laser used for photobleaching).</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="ROIRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
            <xsd:documentation>Description=This empty element has a reference (the ROIID attribute) to a four dimensional ROI ( 'Region of Interest').  If they are not used, and the Image has more than one plane, the entire set of planes is assumed to be included in the ROI. Multiple ROIs may be specified</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="TIRFSettings"/>
        <xsd:element ref="ImagingEnvironment" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element minOccurs="1" ref="MicroscopeSettings" maxOccurs="unbounded"/>
        <xsd:element ref="ObjectiveSettings" minOccurs="1" maxOccurs="unbounded"/>
        <xsd:element minOccurs="0" ref="OpticalCalibration" maxOccurs="unbounded"/>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="DetectorCalibration"/>
        <xsd:sequence>
          <xsd:element ref="Plane" minOccurs="1" maxOccurs="unbounded"/>
          <xsd:element ref="Channel" minOccurs="1" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:appinfo><xsdfu xmlns=""><ordered/></xsdfu></xsd:appinfo>
              <xsd:documentation>Description=Each Image has to have at least one Plane and one Channel. The Channel object stores information about how the each image channel was acquired. The total number of Channel elements for a given Image must equal to SizeC. One useful feature of the model, is that this design allows to create LogicalChannels which are composed of one or more ChannelComponents. For example, an entire spectrum in an FTIR experiment may be stored in a single Logical Channel with each discrete wavenumber of the spectrum constituting a ChannelComponent of the FTIR Logical Channel. An RGB image where the Red, Green and Blue components do not reflect discrete probes but are instead the output of a color camera would be treated similarly - one Logical channel with three ChannelComponents in this case. The IlluminationType attribute is a string enumeration which may be set to 'Transmitted', 'Epifluorescence', 'Oblique', or 'NonLinear'. The user interface logic for labeling a given channel for the user should use the first existing attribute in the following sequence: Name -> Fluor -> EmissionWavelength -> ChannelComponent/Index.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice minOccurs="1" maxOccurs="unbounded">
            <xsd:element ref="BinData" minOccurs="1" maxOccurs="unbounded"/>
            <xsd:element ref="TiffData" minOccurs="1" maxOccurs="unbounded"/>
            <xsd:element ref="MetadataOnly" minOccurs="1" maxOccurs="unbounded"/>
          </xsd:choice>
        </xsd:sequence>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>Description=A description for the image [plain-text multi-line string]
            </xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="ImageID">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this Image.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Name" use="required" type="Denomination">
        <xsd:annotation>
          <xsd:documentation>Description=A user assigned name for this Image.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="AcquisitionDate" type="xsd:date" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=The acquisition date of the Image.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SizeX" use="required" type="PositiveInt">
        <xsd:annotation>
          <xsd:documentation>Description=Dimensional size of pixel data array.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SizeY" use="required" type="PositiveInt">
        <xsd:annotation>
          <xsd:documentation>Desciption=Dimensional size of pixel data array.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SizeZ" use="required" type="PositiveInt">
        <xsd:annotation>
          <xsd:documentation>Description=Dimensional size of pixel data array.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SizeC" use="optional" type="PositiveInt">
        <xsd:annotation>
          <xsd:documentation>Description=Dimensional size of pixel data array.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SizeT" use="required" type="PositiveInt">
        <xsd:annotation>
          <xsd:documentation>Description=Dimensional size of pixel data array.</xsd:documentation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PhysicalSizeX" use="optional" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=Physical size of a pixel.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PhysicalSizeXUnit" use="optional" default="Âµm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PhysicalSizeY" use="optional" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=Physical size of a pixel.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PhysicalSizeYUnit" use="optional" default="Âµm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PhysicalSizeZ" use="optional" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=Physical size of a pixel.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PhysicalSizeZUnit" use="optional" default="Âµm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="TimeIncrement" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=TimeIncrement is used for time series that have a global timing specification instead of per-timepoint timing info. For example in a video stream.
          </xsd:documentation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="TimeIncrementUnit" use="optional" default="s" type="UnitsTime">
        <xsd:annotation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="DimensionOrder" use="required" type="PixelsDimensionOrder">
        <xsd:annotation>
          <xsd:documentation>Description=The order in which the individual planes of data are interleaved.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ChannelOrder" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Description=The order in which the channels were acquired and are stored.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PixelType" use="required" type="PixelType">
        <xsd:annotation>
          <xsd:documentation>Description=The variable type used to represent each pixel in the Image.
          </xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SignificantBits" use="optional" type="PositiveInt">
        <xsd:annotation>
          <xsd:documentation>Description=The number of bits within the type storing each pixel that are significant. e.g. you can store 12 bit data within a 16 bit type. This does not reduce the storage requirements but can be a useful indicator when processing or viewing the image data.
          </xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Interleaved" use="optional" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>Description=How the channels are arranged within the data block: true if channels are stored RGBRGBRGB...; false if channels are stored RRR...GGG...BBB...
          </xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="BigEndian" use="optional" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>Description=This is true if the pixels data was written in BigEndian order. If this value is present it should match the value used in BinData or TiffData. If it does not a reader should honour the value used in the BinData or TiffData. This values is useful for MetadataOnly files and is to allow for future storage solutions.
          </xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Instrument">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Instruments</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=MicroscopyBasics</xsd:documentation>
      <xsd:documentation>Description=This element describes the instrument used to capture the Image. It is primarily a container referring to the microscope's components list, which include the Microscope Body, Microscope Table, LightSource, LightSourceCoupling, SampleHolder, Focusing devices, Objective, Optovar, Filter, Dichroic, AdditionalOptics, Detector and CalibrationTool. The Filter element can be composed either of separate excitation, emission filters and a dichroic mirror or a single filter set. Within the Image itself, a reference is made to this one Filter element. There may be multiple light sources, detectors, objectives and filters etc. in an Instrument. Each of these has their own ID attribute, which can be referred to from Channel. It is understood that the light path configuration can be different for each channel, but cannot be different for each timepoint or each plane of an XYZ stack.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="MicroscopeBody" minOccurs="1" maxOccurs="unbounded"/>
        <xsd:element minOccurs="1" ref="MicroscopeTable"/>
        <xsd:element ref="LightSourceGroup" minOccurs="1" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>Category=LightSource</xsd:documentation>
            <xsd:documentation>Description=This element describes the light source(s) available in this microscope</xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element maxOccurs="unbounded" minOccurs="1" ref="LightSourceCouplingGroup">
          <xsd:annotation>
            <xsd:documentation>Category=LightSourceCoupling</xsd:documentation>
            <xsd:documentation>Description=This element describes the method(s) used to couple the light source with the light path moving through speciment to the detector</xsd:documentation>
            <xsd:documentation>Tier=4</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element maxOccurs="unbounded" minOccurs="1" ref="SamplePositioningGroup">
          <xsd:annotation>
            <xsd:documentation>Category=SampleHolderAndFocusing.Focusing</xsd:documentation>
            <xsd:documentation>Description=This element describes the objective lens and stage positioning device(s) available in this microscope. An instrument may have more than one of these devices. The type of focusing device is specified by one of the child-element which are 'MechanicalStage', 'PiezoElectricSate', or 'PiezoElectricObjective'</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="AutoFocus">
          <xsd:annotation>
            <xsd:documentation>Category=SampleHolderAndFocusing</xsd:documentation>
            <xsd:documentation>Description=This element describes the focus stabilization device(s) available for this microscope</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="Optovar"/>
        <xsd:element ref="Objective" minOccurs="1" maxOccurs="unbounded"/>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="LensGroup"/>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="Prism"/>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="PolarizationOptics"/>
        <xsd:element ref="FilterSet" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="FilterGroup" minOccurs="1" maxOccurs="unbounded"/>
        <xsd:element ref="MirrorGroup" minOccurs="1" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>Description=This element describes mirror component(s) available in this microscope</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="DetectorGroup" minOccurs="1" maxOccurs="unbounded"/>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="IntensityCalibrationTool"/>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
            <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="InstrumentID">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Name" type="Denomination" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=A user defined name for this Instrument.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <!-- Key pixel storage elements -->
  <xsd:element name="Plane">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Planes</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=Each Image has to have at least one Plane and one Channel. The Plane object holds microscope stage and image timing data for a given channel/z-section/timepoint. The total number of Channel elements for a given Image must equal to SizeZ x SizeC x SizeT.
      </xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" ref="SamplePosition" maxOccurs="1"/>
        <xsd:element ref="FocalPosition" minOccurs="1" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>Description=This element is used to record the Z-axis position of a Plane with respect to the microscope's reference frame</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="MechanicalCalibration">
          <xsd:annotation>
            <xsd:documentation>Description=This describes the procedures that were used for mechanical calibration and the resulting calibration results.</xsd:documentation>
            <xsd:documentation>Tier=4</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element minOccurs="0" name="AnnotationRef">
          <xsd:annotation>
            <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference"/>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:choice minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>Description=This optional element is a hash of the plane's image data. It is a choice between all the supported hash types. Currently the only method supported is SHA1
            </xsd:documentation>
          </xsd:annotation>
          <xsd:element name="HashSHA1" type="Hex40" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:sequence>
      <xsd:attribute name="ID" type="LSID">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this Plane.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="TheZ" use="required" type="NonNegativeInt">
        <xsd:annotation>
          <xsd:documentation>Description=The Z-section to which this plane corresponds. This is numbered from 0</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="TheT" use="required" type="NonNegativeInt">
        <xsd:annotation>
          <xsd:documentation>Description=The timepoint to which this plane corresponds. This is numbered from 0</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="TheC" use="required" type="NonNegativeInt">
        <xsd:annotation>
          <xsd:documentation>Description=The channel to which this plane corresponds. This is numbered from 0</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Timestamp" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=This records the total time intercoursed since the beginning of the experiment</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="TimestampUnit" use="optional" default="s" type="UnitsTime">
        <xsd:annotation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ExposureTime" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The length of the exposure</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ExposureTimeUnit" use="optional" default="s" type="UnitsTime">
        <xsd:annotation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Channel">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Channels</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=There must be one per Channel in the Image, even for a single-plane image. Information about how each of the Channel was acquired is stored in the various optional *Ref elements. One useful feature of the model, is that this design allows to create LogicalChannels which are composed of one or more ChannelComponents. For example, an entire spectrum in an FTIR experiment may be stored in a single Logical Channel with each discrete wavenumber of the spectrum constituting a ChannelComponent of the FTIR Logical Channel. An RGB image where the Red, Green and Blue components do not reflect discrete probes but are instead the output of a color camera would be treated similarly - one Logical channel with three ChannelComponents in this case. The total number of ChannelComponents for a set of pixels must equal SizeC. The IlluminationType attribute is a string enumeration which may be set to 'Transmitted', 'Epifluorescence', 'Oblique', or 'NonLinear'. The user interface logic for labeling a given channel for the user should use the first existing attribute in the following sequence: Name -> Fluor -> EmissionWavelength -> ChannelComponent/Index.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="LightSourceSettings" minOccurs="1" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>Description=This holds the setting applied to a LightSource e as well as a reference to the LightSource (the ID attribute) in question. The ID referes to the LightSource used in this case</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="LightPath" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="DetectorSettingsGroup" minOccurs="1" maxOccurs="unbounded"/>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ExcitationCalibration"/>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="FIeldCalibration"/>
        <xsd:element minOccurs="0" name="AnnotationRef">
          <xsd:annotation>
            <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference"/>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="ChannelID">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this Channel.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Name" use="required" type="Denomination">
        <xsd:annotation>
          <xsd:documentation>Description=A name for the channel that is suitable for presentation to the user.
          </xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SamplesPerPixel" use="optional" type="PositiveInt">
        <xsd:annotation>
          <xsd:documentation>Description=The number of samples the detector takes to form each pixel value. [units:none] Note: This is not the same as "Frame Averaging" - see Integration in DetectorSettings.
          </xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="IlluminationType" use="required" type="ChannelIlluminationType">
        <xsd:annotation>
          <xsd:documentation>Description=The method of illumination used to capture the channel.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PinholeSize" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The optional PinholeSize attribute allows specifying adjustable pin-hole diameters for confocal microscopes.
          </xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PinholeSizeUnit" use="optional" default="Âµm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="AcquisitionMode" use="optional" type="ChannelAcquistionMode">
        <xsd:annotation>
          <xsd:documentation>Description=AcquisitionMode describes the type of microscopy performed for each channel.
          </xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ContrastMethod" use="optional" type="ChannelContrastMethod">
        <xsd:annotation>
          <xsd:documentation>Description=ContrastMethod describes the technique used to achieve contrast for each channel.
          </xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ExcitationWavelength" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=The excitation wavelength used for this channel.
          </xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ExcitationWavelengthUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="EmissionWavelength" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=The emission wavelength used for this channel.</xsd:documentation>
          <xsd:documentation>Tier=3
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="EmissionWavelengthUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Fluorophore" use="required" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Description=This is the name of the fluorophore used to produce this channel [plain text string].</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="NDFilter" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The NDfilter attribute is used to specify the combined effect of any neutral density filters used. The amount of light the filter transmits at a maximum. A fraction, as a value from 0.0 to 1.0. NOTE: This was formerly described as "units optical density expressed as a PercentFraction". This was how the field had been described in the schema from the beginning but all the use of it has been in the opposite direction, i.e. as a amount transmitted, not the amount blocked. This change has been made to make the model reflect this usage.
          </xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PockelCellSetting" use="optional" type="xsd:int">
        <xsd:annotation>
          <xsd:documentation>Description=The PockelCellSetting used for this channel. This is the amount the polarization of the beam is rotated by</xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Color" use="optional" type="Color" default="-1">
        <xsd:annotation>
          <xsd:documentation>Description=A color used to render this channel - encoded as RGBA. The default value "-1" is #FFFFFFFF so solid white (it is a signed 32 bit value). NOTE: Prior to the 2012-06 schema the default value was incorrect and produced a transparent red not solid white.
          </xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MetadataOnly">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>MetadataOnlyMarkers</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This place holder means there is on pixel data in this file.
      </xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TiffData">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>TiffDataBlocks</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This describes the location of the pixel data in a tiff file.
      </xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="UUID" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>
              This must be used when the IFDs are located in another file.
              Note: It is permissible for this to be self referential.
            </xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:simpleContent>
              <xsd:extension base="UniversallyUniqueIdentifier">
                <xsd:attribute name="FileName" use="optional" type="xsd:string">
                  <xsd:annotation>
                    <xsd:documentation>
                      This can be used when the IFDs are located in another file.
                      The / (forward slash) is used as the path separator.
                      A relative path is recommended. However an absolute path can be specified.
                      Default is to use the file the ome-xml data has been pulled from.
                      Note: It is permissible for this to be self referential. The file image1.tiff
                      may contain ome-xml data that has FilePath="image1.tiff" or "./image1.tiff"
                    </xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:extension>
            </xsd:simpleContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="IFD" type="NonNegativeInt" default="0" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            Gives the IFD(s) for which this element is applicable. Indexed from 0.
            Default is 0 (the first IFD). [units:none]
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="FirstZ" type="NonNegativeInt" default="0" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            Gives the Z position of the image plane at the specified IFD. Indexed from 0.
            Default is 0 (the first Z position). [units:none]
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="FirstT" type="NonNegativeInt" default="0" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            Gives the T position of the image plane at the specified IFD. Indexed from 0.
            Default is 0 (the first T position). [units:none]
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="FirstC" type="NonNegativeInt" default="0" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            Gives the C position of the image plane at the specified IFD. Indexed from 0.
            Default is 0 (the first C position). [units:none]
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PlaneCount" use="optional" type="NonNegativeInt">
        <xsd:annotation>
          <xsd:documentation>
            Gives the number of IFDs affected. Dimension order of IFDs is given by the enclosing
            Pixels element's DimensionOrder attribute. Default is the number of IFDs in the TIFF
            file, unless an IFD is specified, in which case the default is 1. [units:none]
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <!-- Experimenter elements -->
  <xsd:element name="Leader">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Leaders</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        Contact information for a ExperimenterGroup leader specified using a reference
        to an Experimenter element defined elsewhere in the document.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="ExperimenterID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Rights">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Rights</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        The rights holder of this data and the rights held.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="RightsHolder" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation> The rights holder for this data. [plain-text multi-line string]
              e.g. "Copyright (C) 2002 - 2016 Open Microscopy Environment"
            </xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="RightsHeld" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>
              The rights held by the rights holder. [plain-text multi-line string]
              e.g. "All rights reserved" or "Creative Commons Attribution 3.0 Unported License"
            </xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <!-- Experiemtal description element -->
  <xsd:element name="MicrobeamManipulation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo>Description=Defines a microbeam operation type and the region of the image it was applied to.The LightSourceSettingsRef element is a reference to a LightSourceSettings and in turn to the LightSource specified in the Instrument element which was used for this micromanipulation experiment. For example, a laser used for photobleaching</xsd:appinfo>
      <xsd:documentation>Defines a microbeam operation type and the region of the image it was applied to.
The LightSourceSettingsRef element is a reference to a LightSourceSettings and in turn to the LightSource specified in the Instrument element which was used for this micromanipulation experiment. For example, a laser used for photobleaching.
      </xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ExperimenterRef" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ROIRef" minOccurs="1" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo>Description=This empty element has a reference (the ROIID attribute) to a four dimensional ROI ('Region of Interest') that was used in this micromanipulation experiment</xsd:appinfo>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="LightSourceSettings" minOccurs="1" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>Description=This holds the setting applied to a LightSource e as well as a reference to the LightSource (the ID attribute) in question. The ID referes to the LightSource used in this case</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>Description=A description for the Microbeam Manipulation. [plain-text multi-line string]
            </xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element minOccurs="0" name="AnnotationRef">
          <xsd:annotation>
            <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference"/>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="MicrobeamManipulationID">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Type" use="optional" type="MicrobeamManipulationType">
        <xsd:annotation>
          <xsd:documentation>Description=Defines the type of micromanipulation performed.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TIRFSettings">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>Description=Defines the TIRF settings used for this experiment.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Settings">
          <xsd:sequence>
            <xsd:element name="Description" minOccurs="0" maxOccurs="1">
              <xsd:annotation>
                <xsd:documentation>Description=A description for the settings used for this TIRF experiment [plain-text multi-line string]
                </xsd:documentation>
                <xsd:documentation>Tier=3</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:whiteSpace value="preserve"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="AnnotationRef">
              <xsd:annotation>
                <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
                <xsd:documentation>Tier=3</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:complexContent>
                  <xsd:extension base="Reference"/>
                </xsd:complexContent>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="ID" type="TIRFSettingsID">
            <xsd:annotation>
              <xsd:documentation>Description=A unique identifier for this element.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ThroughLens" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Description=The TIRF modality used in this case. True=Through the lens TIRF modality. False=TIRF modality in which the laser light is not travelling through the objective lens.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="TIRFAngle" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The incidence angle used in this case</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="TIRFAngleUnit" type="UnitsAngle">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Geometry" type="TIRFGeometry">
            <xsd:annotation>
              <xsd:documentation>Description=The TIRF geometry used in this case</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Sample elements -->
  <xsd:element name="Sample">
    <xsd:annotation>
      <xsd:documentation>Description=A description of the sample that was imaged in this experiment.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Description" maxOccurs="1" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Description=A description of the sample that was imaged in this case. [plain-text multi-line string]</xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string"/>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element minOccurs="0" ref="AnnotationRef">
          <xsd:annotation>
            <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" type="LSID">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this Sample.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Name" type="Denomination" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=A user assigned name for this Sample.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MountingMedium" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Description=The mounting medium the sample is embedded in during acquisition.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MountingMediumRefractiveIndex" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The measured refractive index of the medium the sample is embedded in during imaging.</xsd:documentation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ImagingEnvironment">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>ImagingEnvironments</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This describes the environment that the biological sample was in during the experiment
      </xsd:documentation>
      <xsd:documentation>Tier=4</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Map" type="Map" minOccurs="0" maxOccurs="1"/>
      </xsd:sequence>
      <xsd:attribute name="Temperature" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The Temperature at which the imaging experiment was conducted
          </xsd:documentation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="TemperatureUnit" use="optional" default="Â°C" type="UnitsTemperature">
        <xsd:annotation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="AirPressure" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=AirPressure is the define units
          </xsd:documentation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="AirPressureUnit" use="optional" default="mbar" type="UnitsPressure">
        <xsd:annotation>
          <xsd:documentation>Tier=4
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Humidity" use="optional" type="PercentFraction">
        <xsd:annotation>
          <xsd:documentation>Description=Humidity around the sample [units:none]. A fraction, as a value from 0.0 to 1.0.
          </xsd:documentation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CO2Percent" use="optional" type="PercentFraction">
        <xsd:annotation>
          <xsd:documentation>Description=Carbon Dioxide concentration around the sample [units:none]. A fraction, as a value from 0.0 to 1.0
          </xsd:documentation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SamplePosition">
    <xsd:annotation>
      <xsd:documentation>Description=SamplePosition is used to specify a name and location for a specific sample holder position in the microscope's reference frame. This element subsitutes a deprecated element called StageLabel</xsd:documentation>
      <xsd:documentation>Tier=4</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="SamplePositioningRef" maxOccurs="1" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element has a reference (the SampleHolderID attribute) to a SampleHolder element defined within OME</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" type="LSID" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identified for this specific sample holder position.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Name" use="required" type="Denomination">
        <xsd:annotation>
          <xsd:documentation>Description=A user assigned label for this specific sample holder position.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SamplePositionX" use="required" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The X position of the SampleHolder within the microscope's reference frame
          </xsd:documentation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SamplePositionXUnit" use="optional" default="reference frame"
        type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SamplePositionY" use="required" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The Y position of the SampleHolder within the microscope's reference frame</xsd:documentation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SamplePositionYUnit" use="optional" default="reference frame"
        type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SamplePositionZ" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The Z position of the SampleHolder within the microscope's reference frame</xsd:documentation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SamplePositionZUnit" use="optional" default="reference frame"
        type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=4</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FocalPosition">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>StageLabels</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This element is used to record the Z-axis position of a Plane with respect to the microscope's reference frame
      </xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="SamplePositioningRef" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element has a reference (the FocusingID attribute) to a Focusing element defined within OME</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="AutoFocusRef" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element has a reference (the AutofocusID attribute) to a Autofocus element defined within OME</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" type="LSID" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this Z-axis focal position.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Name" use="required" type="Denomination">
        <xsd:annotation>
          <xsd:documentation>Description=A user assigned name for this Z-axis focal position.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="StagePositionZ" use="required" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The Z-axis focal position of this plane.
          </xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="StagePositionZUnit" use="optional" default="reference frame"
        type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier-3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <!-- Microscopy basics elements -->
  <xsd:element name="MicroscopeBody">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Microscopes</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=MicroscopyBasics</xsd:documentation>
      <xsd:documentation>Description=A description of the body of this microscope</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ManufacturerSpec">
          <xsd:sequence>
            <xsd:element ref="AnnotationRef" maxOccurs="1" minOccurs="0"/>
          </xsd:sequence>
          <xsd:attribute name="ID" type="LSID" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Name" type="Denomination" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Type" use="required" type="MicroscopeType">
            <xsd:annotation>
              <xsd:documentation>Description=The type of this microscope.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="EyePieceFieldNumber" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The Field Number represents the diaphragm size of the microscope eyepiece expressed in mm.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MicroscopeTable">
    <xsd:annotation>
      <xsd:documentation>Category=MicroscopyBasics</xsd:documentation>
      <xsd:documentation>Description=This element describes the table on which this Microscope rests during acquisition.</xsd:documentation>
      <xsd:documentation>Tier=4</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ManufacturerSpec">
          <xsd:sequence>
            <xsd:element minOccurs="0" name="AnnotationRef">
              <xsd:annotation>
                <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
                <xsd:documentation>Tier=4</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:complexContent>
                  <xsd:extension base="Reference"/>
                </xsd:complexContent>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="ID" type="LSID" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=4</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Name" type="Denomination" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
              <xsd:documentation>Tier=4</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Type" type="MicroscopeTableType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This element describes the type of microscope table available for this instrument.</xsd:documentation>
              <xsd:documentation>Tier=4</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="TableWidth" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The width of this microscope table</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="TableWidthUnit" type="UnitsLength" default="cm">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="TableDepth" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The depth of this microscope table</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="TableDepthUnit" type="UnitsLength" default="cm">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Sample holder and focusing elements -->
  <xsd:complexType name="SamplePositioning">
    <xsd:annotation>
      <xsd:documentation>Description=This describes a objective lens or stage positioning device available for this instrument. An instrument may have more than one of these devices of different type, such as mechanical stage, piezoelectric stage, objective turret and piezoelectric objective.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ManufacturerSpec">
        <xsd:sequence>
          <xsd:element ref="AnnotationRef" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="ID" type="LSID" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Name" type="Denomination" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ZTravelRange" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The range this device is capable of moving in along the Z axis.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ZTravelRangeUnit" type="UnitsLength" default="Âµm">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Resolution" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The expected motion resolution of this device.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="nm" name="ResolutionUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PositionLinearityError" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The expected positioning error associated with this device. Typically expressed as % error.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Repetability" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The expected precision with which this device is capable of returning to a previously visited position.</xsd:documentation>
            <xsd:documentation>Tier=4</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="nm" name="RepetabilityUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=4</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SettlingTime" type="xsd:float" use="optional">
          <xsd:annotation>
            <xsd:documentation>Description=The expected time it takes for this device to stabilize at the target position.</xsd:documentation>
            <xsd:documentation>Tier=4</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="ms" name="SettlingTimeUnit" type="UnitsTime">
          <xsd:annotation>
            <xsd:documentation>Tier=4</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="MaxVelocity" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=The maximum velocity at which this device is capable of moving.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="nm/s" name="MaxVelocityUnit" type="UnitsVelocity">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="SamplePositioningGroup" abstract="true" type="SamplePositioning">
    <xsd:annotation>
      <xsd:documentation>Description=This describes a objective lens or stage positioning device available for this instrument. An instrument may have more than one of these devices of different type, such as mechanical stage, piezoelectric stage, objective turret and piezoelectric objective.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ObjectiveTurretFocus" substitutionGroup="SamplePositioningGroup">
    <xsd:annotation>
      <xsd:documentation>Category=SamplePositioning.Focusing</xsd:documentation>
      <xsd:documentation>Description=An objective turret positoning device available for this instrument.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="SamplePositioning"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PiezoElectricObjectiveFocus" substitutionGroup="SamplePositioningGroup">
    <xsd:annotation>
      <xsd:documentation>Category=SamplePositioning.Focusing</xsd:documentation>
      <xsd:documentation>Description=An optional piezoelectric objective positioning device associated with this instrument.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="SamplePositioning"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="Stage">
    <xsd:annotation>
      <xsd:documentation>Description=This describes a stage x-y positioning device associated with this instrument.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="SamplePositioning">
        <xsd:attribute name="MovementAxes" use="optional" default="XY" type="StageMovementAxes">
          <xsd:annotation>
            <xsd:documentation>Description=The axes or axis along which this device is capable of moving.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="XTravelRange" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The range this device is capable of moving in along the X axis.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="XTravelRangeUnit" type="UnitsLength" default="Âµm">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="YTravelRange" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The range this device is capable of moving in along the Y axis.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="YTravelRangeUnit" type="UnitsLength" default="Âµm">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element abstract="true" name="StageGroup" substitutionGroup="SamplePositioningGroup"
    type="Stage">
    <xsd:annotation>
      <xsd:documentation>Description=This describes a stage x-y positioning device associated with this instrument.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SampleHolder" substitutionGroup="StageGroup">
    <xsd:annotation>
      <xsd:documentation>Category=SamplePositioning.Stage</xsd:documentation>
      <xsd:documentation>Description=This describes an optional sample holder mounted on the microscope stage.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Stage">
          <xsd:sequence> </xsd:sequence>
          <xsd:attribute name="Type" type="SampleHolderType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The type of this sample holder.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="RotationAngle" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The angle by which this sample holder is capable of rotating.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="RotationAngleUnit" type="UnitsAngle" default="deg">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MechanicalStage" substitutionGroup="StageGroup">
    <xsd:annotation>
      <xsd:documentation>Category=SamplePositioning.Stage</xsd:documentation>
      <xsd:documentation>Description=This describes a mechanical stage positioning device associated with this instrument. This component is not very common but can be found in simple surveying microscopes.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Stage"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PiezoElectricStage" substitutionGroup="StageGroup">
    <xsd:annotation>
      <xsd:documentation>Category=SamplePositioning.Stage</xsd:documentation>
      <xsd:documentation>Description=This describes an optional piezoelectric stage positioning device associated with this instrument.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Stage"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Magnification elements -->
  <xsd:element name="AutoFocus">
    <xsd:annotation>
      <xsd:documentation>Category=SamplePositioning.Focusing</xsd:documentation>
      <xsd:documentation>Description=This element describes one or more optional focus stabilization device(s) available for this instrument.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ManufacturerSpec">
          <xsd:sequence>
            <xsd:element minOccurs="0" name="AnnotationRef">
              <xsd:annotation>
                <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
                <xsd:documentation>Tier=3</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:complexContent>
                  <xsd:extension base="Reference"/>
                </xsd:complexContent>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="ID" type="LSID" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Name" type="Denomination" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Type" type="AutoFocusType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The type of this autofocus device.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Objective">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Objectives</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=Magnification</xsd:documentation>
      <xsd:documentation>Description=A microscope's objective lens is a compound lens consisting of several simple lenses (elements), usually arranged along a common axis.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ManufacturerSpec">
          <xsd:sequence>
            <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="1">
              <xsd:annotation>
                <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="ID" use="required" type="ObjectiveID">
            <xsd:annotation>
              <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Name" type="Denomination" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Correction" use="required" form="unqualified"
            type="ObjectiveCorrection">
            <xsd:annotation>
              <xsd:documentation>Description=The type of optical correction (i.e. chromatict, flatness, etc.) associated with this objective.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="NominalMagnification" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The magnification of the lens as specified by the manufacturer - i.e. 60 represents a 60X lens.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="CalibratedMagnification" use="optional" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The magnification of the lens as measured via a calibration process- i.e. '59.987' for a 60X lens.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="LensNA" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The numerical aperture of this objective expressed as a floating point (real) number. Expected range 0.02 - 1.5.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="NominalImmersionMedium" use="required"
            type="ObjectiveNominalImmersionMedium">
            <xsd:annotation>
              <xsd:documentation>Description=The immersion medium this objective is designed for.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Infinity" type="xsd:boolean" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=Records whether or not the objective is infinity corrected.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="CoverslipTickness" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description = The coverlip thickness this objective is recommended for.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="WorkingDistance" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The working distance of this objective, expressed as a floating point (real) number.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="WorkingDistanceUnit" use="optional" default="mm" type="UnitsLength">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Iris" use="required" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Description=Records whether or not the objective was fitted with an Iris.</xsd:documentation>
              <xsd:documentation>Tier=4</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ObjectiveFieldNumber" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The maximal diameter of the viewfield allowed by this objective expressed in mm and measured at the intermediate image plane. NB: The field of view is influenced by the magnification and diameter of the ocular (eyepiece) field diaphragm, but also by the limits imposed by the design of the objective.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ParfocalizingDistance" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The distance between the objective lens mounting plane and the specimen.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="mm" name="ParfocalizingDistanceUnit" type="UnitsLength"
            use="optional">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="BackFocalPlanePosition" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The position of the back focal plane with respect to the specimen position.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="mm" name="BackFocalPlanePositionUnit" type="UnitsLength"
            use="optional">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Optovar">
    <xsd:annotation>
      <xsd:documentation>Category=Magnification</xsd:documentation>
      <xsd:documentation>Description=Optovars are magnification changing devices originally developed by Zeiss, which provide additional magnification that can be beneficial when a desired field of view is required for the imaging sensor. Magnification changers are placed before the tube lens within the infinity space of the optical system. Typical magnification increments are: 1.25x, 1.6x, and 2x.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ManufacturerSpec">
          <xsd:sequence>
            <xsd:element ref="AnnotationRef" minOccurs="0"/>
          </xsd:sequence>
          <xsd:attribute name="ID" type="LSID" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Name" type="Denomination" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Magnification" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The magnification associated with this magnification changing lens, as specified by the manufacturer.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="LensType" type="LensType" use="optional">
            <xsd:annotation>
              <xsd:documentation>Description=The type of lens associated with this magnification changing device.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Light elements -->
  <xsd:element name="LightSourceGroup" abstract="true" type="LightSource">
    <xsd:annotation>
      <xsd:documentation>Description=This describes the light source(s) available for this instrument. An instrument may have several light sources of different types (Laser, LightEmittingDiode, Filament, Arc etc.). A typical light-microscope has two light sources, one for transmitted light and one for fluorescence light.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="LightSource">
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>LightSources</plural><abstract/></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This describes the light source(s) available for this instrument. An instrument may have several light sources of different types (Laser, LightEmittingDiode, Filament, Arc etc.). A typical light-microscope has two light sources, one for transmitted light and one for fluorescence light.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ManufacturerSpec">
        <xsd:sequence>
          <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
              <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
              <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="ID" use="required" type="LightSourceID">
          <xsd:annotation>
            <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Name" type="Denomination" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="NominalPowerOutput" use="required" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=The average nominal power output of this light-source.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="NominalPowerOutputUnit" use="optional" default="W" type="UnitsPower">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Laser" substitutionGroup="LightSourceGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Lasers</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=LightSource</xsd:documentation>
      <xsd:documentation>Description=A Light Amplification by Stimulated Emission of Radiation (LASER) is a device that emits light through a process of optical amplification based on the stimulated emission of electromagnetic radiation.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Split=[Fluorescence_LightSource]</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="LightSource">
          <xsd:attribute name="Type" use="required" type="LaserType">
            <xsd:annotation>
              <xsd:documentation>Description=Type is the general category of laser.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Pump" type="xsd:boolean" use="required">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
              <xsd:documentation>Description=Wheter or not this is a pumped laser.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="LaserMedium" use="optional" type="LaserMedium">
            <xsd:annotation>
              <xsd:documentation>Description=The Medium attribute specifies the actual lasing medium for a given laser type.           </xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Wavelength" use="required" type="PositiveFloat">
            <xsd:annotation>
              <xsd:documentation>Description=The center Wavelength of the light emitted by this laser.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="WavelengthUnit" use="optional" default="nm" type="UnitsLength">
            <xsd:annotation>
              <xsd:documentation>The units of the Wavelengthm</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="FrequencyMultiplication" use="optional" type="PositiveInt">
            <xsd:annotation>
              <xsd:documentation>Description=The optical frequency multiplication that may be specified for this laser.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Tuneable" use="required" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Description=Whether or not the laser is Tuneable.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Pulse" use="required" type="LaserPulse">
            <xsd:annotation>
              <xsd:documentation>Description=The Pulse mode of the laser.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="PockelCell" use="optional" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Description=If true the laser has a PockelCell to rotate the polarization of the beam.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="RepetitionRate" use="optional" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The is the rate in Hz at which the laser pulses if the Pulse type is 'Repetitive' hertz[Hz]</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="RepetitionRateUnit" use="optional" default="Hz" type="UnitsFrequency">
            <xsd:annotation>
              <xsd:documentation>The units of the RepetitionRate - default:hertz[Hz]</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Arc" substitutionGroup="LightSourceGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Arcs</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=LightSource</xsd:documentation>
      <xsd:documentation>Description=An Arc lamp is a lamp that produces light by an electric arc (also called a voltaic arc), which is typically generated between metal electrodes through an inert gas in a glass bulb.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Split=[Fluorescence_LightSource]</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="LightSource">
          <xsd:attribute name="Type" use="required" type="ArcType">
            <xsd:annotation>
              <xsd:documentation>Description=The type of Arc lamp light source.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Filament" substitutionGroup="LightSourceGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Filaments</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=LightSource</xsd:documentation>
      <xsd:documentation>Description=A Filament lamp is an incandescent light source with a wire filament heated to such a high temperature that it glows with visible light (incandescence).</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Split=[Transmitted_LightSource; Fluorescence_LightSource]</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="LightSource">
          <xsd:attribute name="Type" use="required" type="FilamentType">
            <xsd:annotation>
              <xsd:documentation>Description=The type of Filament lamp light source.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="LightEmittingDiode" substitutionGroup="LightSourceGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>LightEmittingDiodes</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=LightSource</xsd:documentation>
      <xsd:documentation>Description=A Light Emitting Diode (LED) is a semiconductor light source that emits light when current flows through it. Electrons in the semiconductor recombine with electron holes, releasing energy in the form of photons. The color of the light (corresponding to the energy of the photons) is determined by the energy required for electrons to cross the band gap of the semiconductor. White light may be obtained by using multiple semiconductors or a layer of light-emitting phosphor on the semiconductor device.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Split=[Transmitted_LightSource; Fluorescence_LightSource]</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="LightSource">
          <xsd:sequence>
            <xsd:element ref="LEDModule" maxOccurs="unbounded"/>
          </xsd:sequence>
          <xsd:attribute name="Type" type="LightEmittingDiodeType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The type of LightEmittingDiode light source.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LEDModule">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>TransmittanceRanges</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=ChildElement</xsd:documentation>
      <xsd:documentation>Description=A Light Emitting Diode (LED) light source is often composed of multiple modules each with its own wavelenght characteristics. This element describes an individual LED module. Multiple of this elements may be used.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="PeakWavelenght" type="xsd:float" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=The nominal wavelength at which the amount of light power output is at a maximum.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute default="nm" name="PeakWavelenghtUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PeakPower" type="xsd:float" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=The nominal light power output generated at peak wavelenght.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PeakPowerUnit" type="UnitsPower" default="W">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ProjectionAngle" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The angle at which this light source projects light.</xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ProjectionAngleUnit" type="UnitsAngle">
        <xsd:annotation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutIn" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=CutIn is the wavelength below which the response rate is below 50%.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutInUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>The units of the CutIn - default:nanometres[nm]</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutOut" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=CutOut is the wavelength above which the response rate is below 50%</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutOutUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>The units of the CutOut - default:nanometres[nm]</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="WavelenghtProfile" type="xsd:anyURI" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=The location of a file containing the wavelenght profile of this component.</xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>


  <xsd:element name="GenericExcitationSource" substitutionGroup="LightSourceGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>GenericExcitationSources</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=LightSource</xsd:documentation>
      <xsd:documentation>Description=The GenericExcitationSource element is used to represent a light source as a collection of key/value pairs, stored in a Map. One of the other light source elements should always be used in preference to this if possible.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Split=[Transmitted_LightSource; Fluorescence_LightSource]</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="LightSource">
          <xsd:sequence>
            <xsd:element name="Map" type="Map" minOccurs="0" maxOccurs="1">
              <xsd:annotation>
                <xsd:documentation>Category=ChildElement</xsd:documentation>
                <xsd:documentation>Description=This is a Mapping of key/value pairs used to store customized metadata that describes this element.</xsd:documentation>
                <xsd:documentation>Tier=3</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Pump">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Pumps</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=ChildElement</xsd:documentation>
      <xsd:documentation>Description=The Pump element refers to a light source. It is used within the Laser element to specify the light source used as an optional laser pump.</xsd:documentation>
      <xsd:documentation>Tier=5</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="ID" type="LightSourceID"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="LightSourceCouplingGroup" abstract="true" type="LightSourceCoupling">
    <xsd:annotation>
      <xsd:documentation>Description=This element describes the device(s)/method(s) used to couple the available light source(s) with this microscope body in order to transfer light to the specimen. A typical light-microscope has a light coupling device for each available light source.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="LightSourceCoupling">
    <xsd:annotation>
      <xsd:documentation>Description=This element describes the device(s)/method(s) used to couple the available light source(s) with this microscope body in order to transfer light to the specimen. A typical light-microscope has a light coupling device for each available light source.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ManufacturerSpec">
        <xsd:sequence>
          <xsd:element name="CouplingLensRef" type="LensRef" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Description=This is a reference to a lens positioned at the connection between this light source coupling element and the microscope body.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="AnnotationRef">
            <xsd:annotation>
              <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
              <xsd:documentation>Tier=4</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="Reference"/>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="ID" type="LightSourceCouplingID" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
            <xsd:documentation>Tier=4</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Name" type="Denomination" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
            <xsd:documentation>Tier=4</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Transmittance" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The maximum amount of light this LightSourceCoupling apparatus is capable of transmitting at the peak wavelenght. It is represented as a fractional value from 0.0 to 1.0.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="LightGuide" substitutionGroup="LightSourceCouplingGroup">
    <xsd:annotation>
      <xsd:documentation>Category=LightSourceCoupling</xsd:documentation>
      <xsd:documentation>Description=A Light Guide coupling between the light source and the microscope generally consists of a casing enclosing a medium capable of transferring light from the illuminator to the back of the microsocpe body.</xsd:documentation>
      <xsd:documentation>Tier=4</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="LightSourceCoupling">
          <xsd:sequence>
            <xsd:element maxOccurs="1" ref="WavelengthRange" minOccurs="1"/>
          </xsd:sequence>
          <xsd:attribute name="Type" type="LightGuideType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The type of light guide used in this case.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="PeakWavelength" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The wavelength associated with the transmittance peak for this light guide.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="nm" name="PeakWavelengthUnit" type="UnitsLength">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="RefractiveIndex" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The refractive index of the material this light guide is made of.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="NumericalAperture" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The numerical aperture of this light guide expressed as a floating point (real) number. Expected range 0.02 - 1.5.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="AcceptanceAngle" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The angle within which the light beam produced by a light source may be coupled with the bound modes of this light guide.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="AcceptanceAngleUnit" type="UnitsAngle" default="deg">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Geometry" type="LightGuideGeometry">
            <xsd:annotation>
              <xsd:documentation>Description=The shape of this light guide.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="MaterialName" type="LightGuideMaterialName">
            <xsd:annotation>
              <xsd:documentation>Description=The chemical composition or the non-proprietary trade designation of the material used for this light guide.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="SingleMode" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Description=Whether or not this light guide is single-mode. If not, then the guide is assumend to be multi-modal.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="WaveguideMode" type="LightGuideWaveguideMode">
            <xsd:annotation>
              <xsd:documentation>Description=The transverse mode (or superposition of such modes) that are being imposed by this light guide onto the light wave traversing it. These modes generally follow different propagation constants.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Diameter" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The outer diameter of this light guide.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="DiameterUnit" type="UnitsLength" default="mm">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="WavelengthRange">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>TransmittanceRanges</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=ChildElement</xsd:documentation>
      <xsd:documentation>Description=This records a range of wavelengths this device is designed for, according to the manufacturer. Multiple of this elements may be used.</xsd:documentation>
      <xsd:documentation>Tier=5</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="CutIn" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=CutIn is the wavelength below which the response rate is below 50%.</xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutInUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>The units of the CutIn - default:nanometres[nm]</xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutOut" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=CutOut is the wavelength above which the response rate is below 50%.</xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutOutUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>The units of the CutOut - default:nanometres[nm]</xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="WavelenghtProfile" type="xsd:anyURI" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=The location of a file containing the wavelenght profile of this component.</xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Direct" substitutionGroup="LightSourceCouplingGroup">
    <xsd:annotation>
      <xsd:documentation>Category=LightSourceCoupling</xsd:documentation>
      <xsd:documentation>Description=A Direct coupling between the light source and the microscope consists either of a direct mount or of an optical light path that directs light across space to the back of the microscope body.</xsd:documentation>
      <xsd:documentation>Tier=4</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="LightSourceCoupling">
          <xsd:attribute name="Type" type="DirectType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The type of direct coupling available for this Instrument.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Mount" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The mount utilized to couple the available light sources to this microscope body.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Ligth path elements -->
  <xsd:element name="LightPath">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>LightPaths</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=A description of the light path.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="LightSourceCouplingRef">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element can be used (via the required LightSourceCouplingID attribute) to refer to a LightSourceCoupling element defined within OME.</xsd:documentation>
            <xsd:documentation>Tier=4</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="FilterSetRef">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element has a reference (the FilterSet ID attribute) to the FilterSet placed in the light path</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="ExcitationFilterRef" type="FilterRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><plural>ExcitationFilters</plural><parentordered/><manytomany/></xsdfu></xsd:appinfo>
            <xsd:documentation>Description=This empty element has a reference (the Filter ID attribute) to the Filters placed in the Excitation light path</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element minOccurs="0" maxOccurs="unbounded" ref="StandardDichroicRef">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element has a reference (the Mirror ID attribute) to the Dichroic utilized in this Filter Set.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="EmissionFilterRef" type="FilterRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><plural>EmissionFilters</plural><manytomany/></xsdfu></xsd:appinfo>
            <xsd:documentation>Description=This empty element has a reference (the Filter ID attribute) to the Filters placed in the Emission light path</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element minOccurs="0" ref="AdditionalOptics"/>
        <xsd:element minOccurs="0" name="AnnotationRef">
          <xsd:annotation>
            <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference"/>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this element.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Name">
        <xsd:annotation>
          <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <!-- Filter elements -->
  <xsd:element name="FilterSet">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>FilterSets</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=LightPath</xsd:documentation>
      <xsd:documentation>Description=A filter set is typically composed of one excitation filter, one emission filter and a dichroic. They might either be pre-assebled by the manufacturer and pruchased as a whole or they can be custome assembled on site depending on the needs of the user.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ManufacturerSpec">
          <xsd:sequence>
            <xsd:element name="ExcitationFilterRef" type="FilterRef" minOccurs="0" maxOccurs="1">
              <xsd:annotation>
                <xsd:appinfo><xsdfu xmlns=""><plural>ExcitationFilters</plural><manytomany/></xsdfu></xsd:appinfo>
                <xsd:documentation>Description=This refers to an available excitation filter, which is part of this filter set.</xsd:documentation>
                <xsd:documentation>Tier=2</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" ref="StandardDichroicRef">
              <xsd:annotation>
                <xsd:documentation>Description=This refers to an available dichroic mirror, which is part of this filter set.</xsd:documentation>
                <xsd:documentation>Tier=2</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="EmissionFilterRef" minOccurs="0" maxOccurs="1" type="FilterRef">
              <xsd:annotation>
                <xsd:appinfo><xsdfu xmlns=""><plural>EmissionFilters</plural><manytomany/></xsdfu></xsd:appinfo>
                <xsd:documentation>Description=This refers to an available emission filter, which is part of this filter set.</xsd:documentation>
                <xsd:documentation>Tier=2</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="AnnotationRef"/>
          </xsd:sequence>
          <xsd:attribute name="ID" use="required" type="FilterSetID">
            <xsd:annotation>
              <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Name" type="Denomination" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:complexType name="FIlter">
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Filters</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This describes an optical filter positioned in the light path for one or more channel available for this instrument. An optical filter is a device that selectively transmits light of different wavelengths. An instrument may have several filters of different types, such as an excitation, an emission filter or an additional filter positioned in the light path for different purposes. For each fluorescence Image there should be at least one filter element specified per Channel. This element can be associated with an optional FilterWheel - Note: FilterWheel is not the same as a FilterSet.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ManufacturerSpec">
        <xsd:sequence>
          <xsd:element ref="TransmittanceRange" minOccurs="1" maxOccurs="unbounded"/>
          <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
              <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="ID" use="required" type="FilterID">
          <xsd:annotation>
            <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Name" type="Denomination" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Type" use="optional" type="FilterType" default="BandPass">
          <xsd:annotation>
            <xsd:documentation>Description=The type of filter used in this case.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReflectionCoating" type="xsd:string" use="optional">
          <xsd:annotation>
            <xsd:documentation>Description=An optical coating is one or more thin layers of material deposited on an optical component such as a lens or mirror, which alters the way in which the optic reflects and transmits light.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FilterWheel" use="optional" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Description=This can can refer to any arrangement of filters in a filter holder of any shape. It could, for example, be a filter slider.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TransmittanceProfileFile" type="xsd:anyURI" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=This contains the location of file in .csv or other suitable formats containing the wavelenght profile of this component.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>

  </xsd:complexType>

  <xsd:element name="FilterGroup" abstract="true" type="FIlter">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Filters</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This describes an optical filter positioned in the light path for one or more channel available for this instrument. An optical filter is a device that selectively transmits light of different wavelengths. An instrument may have several filters of different types, such as an excitation, an emission filter or an additional filter positioned in the light path for different purposes. For each fluorescence Image there should be at least one filter element specified per Channel. This element can be associated with an optional FilterWheel - Note: FilterWheel is not the same as a FilterSet.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>

  <xsd:element name="ExcitationFilter" substitutionGroup="FilterGroup">
    <xsd:annotation>
      <xsd:documentation>Category=LightPath</xsd:documentation>
      <xsd:documentation>Description=An optical filter is a device that selectively transmits light of different wavelengths.This element refers to an excitation filter. This element can be associated with an optional FilterWheel - Note: FilterWheel is not the same as a FilterSet.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="FIlter"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="EmissionFilter" substitutionGroup="FilterGroup">
    <xsd:annotation>
      <xsd:documentation>Category=LightPath</xsd:documentation>
      <xsd:documentation>Description=An optical filter is a device that selectively transmits light of different wavelengths. This element refers to an emission filter. This element can be associated with an optional FilterWheel - Note: FilterWheel is not the same as a FilterSet.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="FIlter"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="AdditionalFilter" substitutionGroup="FilterGroup">
    <xsd:annotation>
      <xsd:documentation>Category=AdditionalOptics</xsd:documentation>
      <xsd:documentation>Description=An optical filter is a device that selectively transmits light of different wavelengths. This element refers to a filter positioned along the light path in addition to the excitation and the emission filters.This element can be associated with an optional FilterWheel - Note: FilterWheel is not the same as a FilterSet.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="FIlter"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TransmittanceRange">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>TransmittanceRanges</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=ChildElement</xsd:documentation>
      <xsd:documentation>Description=This records the range of wavelengths that are transmitted by the filter. It also records the maximum amount of light transmitted. Multiple of this elements can be used.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="CutIn" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=CutIn is the wavelength below which the response rate is below 50%.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutInUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutOut" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=CutOut is the wavelength above which the response rate is below 50%.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutOutUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Transmittance" use="required" type="PercentFraction">
        <xsd:annotation>
          <xsd:documentation>Description=The maximum amount of light this optical component is capable of transmitting at the peak wavelenght. It is represented as a fractional value from 0.0 to 1.0.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <!-- Mirror elements -->
  <xsd:complexType name="Mirror">
    <xsd:annotation>
      <xsd:documentation>Description=This describes a mirror positioned in the light path for one or more channel available for this instrument. An instrument may have several mirrors of different types, such as dichroic, beamsplitter or reflector.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ManufacturerSpec">
        <xsd:sequence>
          <xsd:element ref="AnnotationRef" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="ID" type="MirrorID" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Name" type="Denomination" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AngleOfIncidence" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the optimal angle of incidence (AOI) for this mirror. TheAOI is the angle between the optical axis of the incident light and the axis normal to the surface of the mirror. In specular reflection AOI is by definition equal to the Angle Of Reflection (AOR).</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="deg" name="AngleOfIncidenceUnit">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the optimal angle of incidence (AOI) for this mirror. TheAOI is the angle between the optical axis of the incident light and the axis normal to the surface of the mirror. In specular reflection AOI is by definition equal to the Angle Of Reflection (AOR).</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="UnitsAngle">
              <xsd:enumeration value="deg"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="MirrorType" type="MirrorType">
          <xsd:annotation>
            <xsd:documentation>Description=The general category this mirror belongs to.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Geometry" type="MirrorGeometry">
          <xsd:annotation>
            <xsd:documentation>Description=The geometrical shape of the surface(s) of this mirror.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RadiusOfCurvature" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=The radius of curvature of this mirror. A spherical lens or mirror surface has a center of curvature located either along or decentered from the system local optical axis. The vertex of the lens surface is located on the local optical axis. The distance from the vertex to the center of curvature is the radius of curvature of the surface.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RadiusOfCurvatureUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SubstrateType" type="SubstrateType">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the type of substrate that is used for the beamsplitter coating. It is typically a ground and polished piece of optical glass.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SubstrateMaterial" type="SubstrateMaterialName">
          <xsd:annotation>
            <xsd:documentation>Description=This records the chemical composition or the nonproprietary trade designation of the mirror's optical substrate. For dichoric mirrors the substrate is typically composed of a glass material with low autofluorescence, such as UV-grade fused silica.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="GlassCode" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Description=If available provide the international glass code for the glass this polarization device is made of. This is based on U.S. military standard MIL-G-174, and is a six-digit number specifying the glass according to its refractive index nd at the Fraunhofer d- (or D3-) line, and its Abbe number Vd also taken at that line. The resulting glass code is the value of nd-1 rounded to three digits, followed by Vd rounded to three digits, with all decimal points ignored. For example, BK7 has nd = 1.5168 and Vd = 64.17, giving a six-digit glass code of 517642.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="MirrorGroup" abstract="true" type="Mirror">
    <xsd:annotation>
      <xsd:documentation>Description=This describes a mirror positioned in the light path for one or more channel available for this instrument. An instrument may have several mirrors of different types, such as dichroic, beamsplitter or reflector.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="BeamSplitter" substitutionGroup="MirrorGroup">
    <xsd:annotation>
      <xsd:documentation>Category=Mirror</xsd:documentation>
      <xsd:documentation>Description=This is a mirror that reflects part of the light, regardless of wavelenght.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Mirror">
          <xsd:attribute name="Transmittance" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The maximum amount of light this beamsplitteris capable of transmitting at the peak wavelenght. It is represented as a fractional value from 0.0 to 1.0.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="TransmissionAngle" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The angle at which light is transmitted by this beamsplitter.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Reflectance" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The maximum amount of light this Mirror is capable of reflecting. It is represented as a fractional value from 0.0 to 1.0.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Reflector" substitutionGroup="MirrorGroup">
    <xsd:annotation>
      <xsd:documentation>Category=Mirror</xsd:documentation>
      <xsd:documentation>Description=This is a total reflection mirror.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Mirror">
          <xsd:attribute name="Reflectance" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The maximum amount of light this Mirror is capable of reflecting. It is represented as a fractional value from 0.0 to 1.0.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:complexType name="Dichroic">
    <xsd:annotation>
      <xsd:documentation>Description=A dichroic beamsplitter or dichroic mirror is a mirror that spectrally separate light by transmitting and reflecting light as a function of wavelength. In the context of fluorescence microscopy, they are typically used to reflect the excitation light, but transmit the emitted fluorescence. Dichroic mirrors can be categorized in different manners. However, the most important criteria for choosing a dichoric mirror are the transmittance and the reflectance wavelenght ranges.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="Mirror">
        <xsd:sequence>
          <xsd:element maxOccurs="unbounded" ref="TransmittanceRange"/>
          <xsd:element maxOccurs="unbounded" ref="ReflectanceRange" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="DichroicType" type="DichroicType" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The type of dichroic mirror used in this case.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="HardSputtered" type="xsd:boolean" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=This field records whether this coating of this dichroic is hard-sputtered or traditional.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AttenuationCoefficient" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The attenuation coeffieient of this dichroic The attenuation coefficient (aka blocking coefficient) is used as a measure of the out-of-band attenuation of an optical filter or dichroic mirror over an extended range of the spectrum. The attenuation coefficient is often defined in units of optical density (OD), wherease OD= -log10(T), and T=Transmittance (0â¤Tâ¤1).</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="Reflection" name="AttenuationMethod" type="AttenuationMethod">
          <xsd:annotation>
            <xsd:documentation>Description=The mean by which this dichroic filter achieves out-of-band transmission attenuation.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Technology" type="TechnologyType">
          <xsd:annotation>
            <xsd:documentation>Description=The filtering technology utilized by this dichroic. The main types of filter technology used in fluorescence analysis are absorption (aka colored) glass, thin-film coating, and acousto-optical tunable, liquid-crystal tunable filters.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Polarization" type="DichroicPolarization">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the polarization effect of this dichroic. Dichroic beamsplitters (and, in fact, any thin-film interference coating that is used at non-normal angles-of-incidence) will cause some amount of polarization. ), the precise effect varying greatly with wavelength and with the particular coating design.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FilterWheel" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Description=This can can refer to any arrangement of filters in a filter holder of any shape. It could, for example, be a filter slider.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TransmittanceProfileFile" type="xsd:anyURI" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=This contains the location of a file in .csv or other suitable formats containing the wavelenght profile of this component.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReflectanceProfileFile" type="xsd:anyURI" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=This contains the location of a file in .csv or other suitable formats containing the wavelenght profile of this component.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:element abstract="true" name="DichroicGroup" substitutionGroup="MirrorGroup" type="Dichroic">
    <xsd:annotation>
      <xsd:documentation>Description=A dichroic beamsplitter or dichroic mirror is a mirror that spectrally separate light by transmitting and reflecting light as a function of wavelength. In the context of fluorescence microscopy, they are typically used to reflect the excitation light, but transmit the emitted fluorescence. Dichroic mirrors can be categorized in different manners. However, the most important criteria for choosing a dichoric mirror are the transmittance and the reflectance wavelenght ranges.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="StandardDichroic" substitutionGroup="DichroicGroup">
    <xsd:annotation>
      <xsd:documentation>Category=LightPath</xsd:documentation>
      <xsd:documentation>Description=This describes a dichroic  positioned in the standard light path for one or more channel available for this instrument.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Dichroic"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="AdditionalDichroic" substitutionGroup="DichroicGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Dichroics</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=AdditionalOptics</xsd:documentation>
      <xsd:documentation>Description=This describes an additional dichroic  positioned in the light path for one or more channel available for this instrument.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Dichroic"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ReflectanceRange">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>TransmittanceRanges</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=ChildElement</xsd:documentation>
      <xsd:documentation>Description=This records the range of wavelengths that are reflected by the mirror.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="CutIn" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=CutIn is the wavelength below which the response rate is below 50%.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutInUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutOut" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=CutOut is the wavelength above which the response rate is below 50%.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutOutUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Reflectance" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=The maximum amount of light this Mirror is capable of reflecting. It is represented as a fractional value from 0.0 to 1.0.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <!-- Additional optics elements -->
  <xsd:element name="AdditionalOptics">
    <xsd:annotation>
      <xsd:documentation>Description=A description of additional optics placed in the light path</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="AdditionalLensRef" type="LensRef" maxOccurs="unbounded" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element has a reference (the LensID attribute) to the an additional Lens placed in the light path</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="PrimsRef" type="PrismRef" maxOccurs="unbounded" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element has a reference (the PrismID attribute) to the a Prism placed in the light path</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="PolarizationOpticsRef" type="PolarizationOpticsRef" maxOccurs="unbounded"
          minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element has a reference (the PorarizationOpticsID attribute) to the a PolarizationOptics placed in the light path</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="AdditionalFilterRef" type="FilterRef" maxOccurs="unbounded" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element has a reference (the FilterID attribute) to the an additional Filter placed in the light path</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="AdditionalMirrorRef" type="MirrorRef" maxOccurs="unbounded" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Description=This empty element has a reference (the MirrorID attribute) to the an additional Mirror placed in the light path</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="AnnotationRef"/>
      </xsd:sequence>
      <xsd:attribute name="ID" type="LSID" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=A unique identifier for this element.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Name" type="Denomination">
        <xsd:annotation>
          <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:complexType name="Lens">
    <xsd:annotation>
      <xsd:documentation>Description=This describes a lens positioned in the light path for one or more channel available for this instrument. A lens is a component of glass or transparent plastic material, usually circular in diameter, which has two primary surfaces that are ground and polished in a specific manner designed to produce either a convergence or divergence of light passing through the material. An instrument may have several lenses with different fuctions such as, a coupling lens positioned between the light source and the microscope body, a relay lens positioned at the detector mount, or an additional lens positioned in the light path for various purposes.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ManufacturerSpec">
        <xsd:sequence>
          <xsd:element ref="AnnotationRef" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="ID" type="LensID" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Name" type="Denomination" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Type" type="LensType" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The general category this lens belongs to.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Magnification" type="xsd:int" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The nominal magnification provided by this lens as specified by the manufacturer.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Construction" type="LensConstruction" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=Whether or not this lens is simple or composite.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Geometry" type="LensGeometry" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The geometrical shape of this lens' surface(s).</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RefractiveIndex" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The refractive index of the material this lens is made of, as specified by the manufacturer.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AttenuationCoefficient" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The attenuation coeffieient of this lens. The attenuation coefficient (aka blocking coefficient) is a material property that characterizes the degree of transparency of a lens. The attenuation coefficient is often defined in units of optical density (OD), wherease OD= -log10(T), and T=Transmittance (0â¤Tâ¤1).</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FocalDistanceToObject" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The distance between the front principal plane of this lens and the specimen plane.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FocalDistanceToObjectUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FocalDistanceToImage" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The distance between the rear principal plane of this lens and the image plane.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FocalDistanceToImageUnits" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FocalLength" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The distance from the rear principal plane of this lens and its focal plane.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FocalLengthUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Diameter" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=The physical diamter of this lens.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DiameterUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AbbeNumber" type="xsd:float" use="optional">
          <xsd:annotation>
            <xsd:documentation>Description=The Abbe number of this lens. The Abbe Number is a measure of the material's dispersion (variation of refractive index versus wavelength), with high values indicating low dispersion.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RadiusOfCurvature" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=The radius of curvature of this lens. A spherical lens or mirror surface has a center of curvature located either along or decentered from the system local optical axis. The vertex of the lens surface is located on the local optical axis. The distance from the vertex to the center of curvature is the radius of curvature of the surface.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RadiusOfCurvatureUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="MaterialName" type="LensMaterialName">
          <xsd:annotation>
            <xsd:documentation>Description=This records the chemical composition or the nonproprietary trade designation of the material this lens is made of.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="GlassCode" type="xsd:short">
          <xsd:annotation>
            <xsd:documentation>Description=If available provide the international glass code for the glass this lens is made of. This is based on U.S. military standard MIL-G-174, and is a six-digit number specifying the glass according to its refractive index nd at the Fraunhofer d- (or D3-) line, and its Abbe number Vd also taken at that line. The resulting glass code is the value of nd-1 rounded to three digits, followed by Vd rounded to three digits, with all decimal points ignored. For example, BK7 has nd = 1.5168 and Vd = 64.17, giving a six-digit glass code of 517642.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Density" type="xsd:float" use="optional">
          <xsd:annotation>
            <xsd:documentation>Description=The density of the material this lens is made of.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DensityUnit" type="UnitsDensity">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:element name="LensGroup" abstract="true" type="Lens">
    <xsd:annotation>
      <xsd:documentation>Description=This describes a lens positioned in the light path for one or more channel available for this instrument. A lens is a component of glass or transparent plastic material, usually circular in diameter, which has two primary surfaces that are ground and polished in a specific manner designed to produce either a convergence or divergence of light passing through the material. An instrument may have several lenses with different fuctions such as, a coupling lens positioned between the light source and the microscope body, a relay lens positioned at the detector mount, or an additional lens positioned in the light path for various purposes.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>

  <xsd:element name="AdditionalLens" substitutionGroup="LensGroup">
    <xsd:annotation>
      <xsd:documentation>Category=AdditionalOptics</xsd:documentation>
      <xsd:documentation>Description=Any additional lens poistioned in the light path for a given channel.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Lens"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CouplingLens" substitutionGroup="LensGroup">
    <xsd:annotation>
      <xsd:documentation>Category=AdditionalOptics</xsd:documentation>
      <xsd:documentation>Description=A lens positioned at connector between this light source coupling device and the microscope body.</xsd:documentation>
      <xsd:documentation>Tier=5</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Lens"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="RelayLens" substitutionGroup="LensGroup">
    <xsd:annotation>
      <xsd:documentation>Category=AdditionalOptics</xsd:documentation>
      <xsd:documentation>Description=A lens positioned at the mount connecting a detector to the microscope body.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Lens"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Prism">
    <xsd:annotation>
      <xsd:documentation>Category=AdditionalOptics</xsd:documentation>
      <xsd:documentation>Description=This describes a prism positioned in the light path for one or more channel available for this instrument. A prism is a block of glass or other transparent material with flat, polished surfaces, which can be viewed as an ensamble of different rigidly mounted mirrors, with each face having a permanent orientation with respect to all the others. A prism can be employed to deflect or deviate a light beam, rotate or invert an image, separate polarization states, or disperse light into its component wavelengths. An instrument may have several prisms with different factionality such as,  reflecting, polarizing, and refracting or dispersion prisms.
Reflecting prisms are useful for redirecting light beams by total internal reflection. Polarizing prisms are birefringent crystals that divide incident non-polarized light into separate components polarized orthogonally to each other. Refracting prisms can be employed to bend and separate light into its component colors. These prisms are employed to produce polarized light for optical instruments such as microscopes and polarimeters.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ManufacturerSpec">
          <xsd:sequence>
            <xsd:element ref="WavelengthRange"/>
            <xsd:element ref="AnnotationRef" maxOccurs="1" minOccurs="0"/>
          </xsd:sequence>
          <xsd:attribute name="ID" type="PrismID" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Name" type="Denomination" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Type" type="PrismType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The category to which this prism belongs.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="RefractiveIndex" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The refractive index of the material this prism is made of, as specified by the manufacturer.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Reflectance" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The maximum amount of light this Prism is capable of transmitting at the peak wavelenght. It is represented as a fractional value from 0.0 to 1.0.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="AngleOfIncidence" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The angle of incidence (AOI) is the angle between a beam of light incident on the prism surface and the line perpendicular to the surface at the point of incidence, called the normal.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="AngleOfIncidenceUnit" type="UnitsAngle">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="DeviationAngle" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A beam passing through an object like a prism is deflected twice: once entering, and again when exiting. The sum of these two deflections is called the deviation angle and is measured with respect to the incident ray.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="DevianceAngleUnit" type="UnitsAngle">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Geometry" type="PrismGeometry">
            <xsd:annotation>
              <xsd:documentation>Description=The geometrical shape of this prism.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="AbbeNumber" type="xsd:float" use="optional">
            <xsd:annotation>
              <xsd:documentation>Description=The Abbe Number is a measure of the material's dispersion (variation of refractive index versus wavelength), with high values indicating low dispersion</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="PrismAngle" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The angle between two surfaces of this prism.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="PrismAngleUnit" type="UnitsAngle">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="MaterialName" type="PrismMaterialName" use="optional">
            <xsd:annotation>
              <xsd:documentation>Description=This records the chemical composition or the nonproprietary trade designation of the material this prism is made of.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="GlassCode" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Description=If available provide the international glass code for the glass this prism is made of. This is based on U.S. military standard MIL-G-174, and is a six-digit number specifying the glass according to its refractive index nd at the Fraunhofer d- (or D3-) line, and its Abbe number Vd also taken at that line. The resulting glass code is the value of nd-1 rounded to three digits, followed by Vd rounded to three digits, with all decimal points ignored. For example, BK7 has nd = 1.5168 and Vd = 64.17, giving a six-digit glass code of 517642.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Density" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The density of the material of which the prism is made.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="DensityUnit" type="UnitsDensity">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="PolarizationOptics">
    <xsd:annotation>
      <xsd:documentation>Category=AdditionalOptics</xsd:documentation>
      <xsd:documentation>Description=This describes an optical polarization device positioned in the light path for one or more channel available for this instrument. A polarization device is an optical filter that lets light waves of a specific polarization pass through while blocking light waves of other polarizations.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ManufacturerSpec">
          <xsd:sequence>
            <xsd:element maxOccurs="1" ref="WavelengthRange" minOccurs="1"/>
            <xsd:element ref="AnnotationRef" minOccurs="0"/>
          </xsd:sequence>
          <xsd:attribute name="ID" type="PolarizationOpticsID" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Name" type="Denomination" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Function" type="PolarizationOpticsFunction" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field records whether this polarizer optics is used to generate or to analyze polarized light.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Construction" type="PolarizationOpticsConstruction">
            <xsd:annotation>
              <xsd:documentation>Description=The modality with which this polarization device is constructed.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Fabrication" type="PolarizationOpticsFabrication">
            <xsd:annotation>
              <xsd:documentation>Description=The way in with which this polarization device is fabricated.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Retardation" type="PolarizationOpticsRetardation">
            <xsd:annotation>
              <xsd:documentation>Description=Retardation describes the phase shift between the polarization component projected along the fast axis and the component projected along the slow axis</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="MaterialName" type="PolarizationOpticsMaterialName">
            <xsd:annotation>
              <xsd:documentation>Description=This records the chemical composition or the nonproprietary trade designation of the material this polarization device is made of.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="GlassCode" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Description=If available provide the international glass code for the glass this polarization device is made of. This is based on U.S. military standard MIL-G-174, and is a six-digit number specifying the glass according to its refractive index nd at the Fraunhofer d- (or D3-) line, and its Abbe number Vd also taken at that line. The resulting glass code is the value of nd-1 rounded to three digits, followed by Vd rounded to three digits, with all decimal points ignored. For example, BK7 has nd = 1.5168 and Vd = 64.17, giving a six-digit glass code of 517642.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Detector elements -->
  <xsd:complexType name="Detector">
    <xsd:annotation>
      <xsd:documentation>Description=This describes a detector available for this instrument. An instrument may have more than one detector of different types, such as photomultiplier, or camera. The Detector ID can be used as a reference within the Channel element in the Image element. The values stored in Detector represent the fixed values, variable values modified during the acquisition go in DetectorSettings.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ManufacturerSpec">
        <xsd:sequence>
          <xsd:element maxOccurs="1" ref="WavelengthRange" minOccurs="1"/>
          <xsd:element name="RelayLensRef" type="LensRef" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
              <xsd:documentation>Description=This is a reference to a lens positioned at the mount connecting this detector to the microscope body.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="AnnotationRef" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="ID" type="DetectorID" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Name" type="Denomination" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A user defined name for this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Gain" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The analog gain is the conversion unit for this detector, as specified by the manufacturer. It specifies how many electrons correspond to 1 signal unit. It is expressed in e-/Analog-to-Digit-Unit (ADU).</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="C-mount" name="Mount" type="DetectorMount" use="optional">
          <xsd:annotation>
            <xsd:documentation>Description=The type of mount used to connect this detector to the microscope body.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReadNoise" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The read noise of this detector, as specified by the manufacturer. Read noise includes thermal noise build-up and clock induced charge. This is generally expressed in e- rms.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="e- rms" name="ReadNoiseUnit" type="UnitsElectricCurrent">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DetectorNoiseModel" type="DetectorNoiseModel" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The type of noise associated with this detector.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DarkCurrentRate" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The dark current rate of this detector as specified by the manufacturer. The dark current rate is the median current detected in the absence of light. Generally expressed in e-/pixel/sec.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="e-/pixel/sec" name="DarkCurrentRateUnit" type="UnitsElectricCurrent">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PeakWavelength" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The wavelength associated with maximum light intensity detected by this device.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PeakWavelenghtUnit" type="UnitsLength" default="nm">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="QuantumEfficiencyatPeakWavelength" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The quantum efficiency observed at peak wavelength, according to the manufacturer. Quantum Efficiency is generally defined as is the ratio of incident photon to converted electrons. Typically expressed as a number between 0 and 1.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AmbientOperatingTemperature" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=The ambient temperature at which this detector is designed to operate, according to the manufactuer.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="Â°C" name="AmbientOperatingTemperatureUnit" type="UnitsTemperature">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AmbientOperatingHumidity" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=The ambient humidity at which this detector is designed to operate, according to the manufactuer.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="DetectorGroup" abstract="true" type="Detector">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Detectors</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This describes a detector available for this instrument. An instrument may have more than one detector of different types, such as photomultiplier, or camera. The Detector ID can be used as a reference within the Channel element in the Image element. The values stored in Detector represent the fixed values, variable values modified during the acquisition go in DetectorSettings.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotoMultiplier" substitutionGroup="DetectorGroup">
    <xsd:annotation>
      <xsd:documentation>Category=Detector</xsd:documentation>
      <xsd:documentation>Description=A photomultiplier tube, useful for light detection of very weak signals, is a photoemissive device in which the absorption of a photon results in the emission of an electron. These detectors work by amplifying the electrons generated by a photocathode exposed to a photon flux.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Detector">
          <xsd:attribute name="Type" type="PhotoMultiplierType">
            <xsd:annotation>
              <xsd:documentation>Description=The type of photomultiplier used.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="RadiantSensitivity" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The radiant sensitivity observed with this PhotoMultiplier at peak wavelength.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="GenericDetector" substitutionGroup="DetectorGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>GenericExcitationSources</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Category=Detector</xsd:documentation>
      <xsd:documentation>Description=This element is used to represent a detector as a collection of key/value pairs, which are stored in a Map. One of the other detector elements should always be used in preference to this if possible.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Detector">
          <xsd:sequence>
            <xsd:element name="Map" type="Map">
              <xsd:annotation>
                <xsd:documentation>Category=ChildElement</xsd:documentation>
                <xsd:documentation>Description=This is a Mapping of key/value pairs used to store customized metadata that describes this element.</xsd:documentation>
                <xsd:documentation>Tier=3</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CameraGroup" abstract="true" type="Camera" substitutionGroup="DetectorGroup">
    <xsd:annotation>
      <xsd:documentation>Description=This describes a camera device available for this instrument. A camera is device that converts optical brightness into an electrical signal using a CCD(a semiconductor chip, which converts the intensity of light falling onto its surface at different points into a charge) and then reproduces an image of the specimen.  An instrument may have more than one camera of different types, such as EMCCD, intensified EMCCD, CMOS, or analog video.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="Camera">
    <xsd:annotation>
      <xsd:documentation>Description=This describes a camera device available for this instrument. A camera is device that converts optical brightness into an electrical signal using a CCD(a semiconductor chip, which converts the intensity of light falling onto its surface at different points into a charge) and then reproduces an image of the specimen.  An instrument may have more than one camera of different types, such as EMCCD, intensified EMCCD, CMOS, or analog video.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="Detector">
        <xsd:attribute name="Illumination" type="CameraIllumination" default="Back" use="optional">
          <xsd:annotation>
            <xsd:documentation>Description=The type of illumination the camera chip receives</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ArrayWidth" type="xsd:int" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The width of this camera chip expressed in pixel.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ArrayHeight" type="xsd:int" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The height of this camera chip expressed in pixel.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PixelWidth" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The physical width of each pixel.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="Âµm" name="PixelWidthUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PixelHeight" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The physical height of each pixel.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="Âµm" name="PixelHeightUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ImageWidth" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=The width of the image generated by the camera.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="Âµm" name="ImageWidthUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ImageHeight" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=The width of the image generated by the camera.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="Âµm" name="ImageHeightUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PixelWellCapacity" type="xsd:int" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The maximum amount of charge each pixel well can hold, measured in electron counts.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="e-" name="PixelWellCapacityUnit" type="UnitsElectricCharge">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RegisterWellCapacity" type="xsd:int">
          <xsd:annotation>
            <xsd:documentation>Description=The maximum amount of charge each register well can hold, measured in electron counts.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="e-" name="RegisterWellCapacityUnit" type="UnitsElectricCharge">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="VerticalClockSpeed" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=The maximal speed each vertical colunm of pixels in the camera chip, can be shifted down and read.

</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="Âµs" name="VerticalClockSpeedUnit" type="UnitsTime">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="MaximumFrameRate" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The maximal rate at which this camera can acquire a single frame.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="fps" name="MaximumFrameRateUnit" type="UnitsFrequency">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="MaximumReadoutRate" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=The maximal pixel read out rate allowed by this camera.</xsd:documentation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="MHz" name="MaximumReadoutRateUnit" type="UnitsFrequency">
          <xsd:annotation>
            <xsd:documentation>Tier=5</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="CCD" substitutionGroup="CameraGroup">
    <xsd:annotation>
      <xsd:documentation>Category=Detector.Camera</xsd:documentation>
      <xsd:documentation>Description=A charged coupled device camera.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Camera">
          <xsd:attribute name="Type" type="CCDType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The type of CCD camera used.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="IntensifiedCCD" substitutionGroup="CameraGroup">
    <xsd:annotation>
      <xsd:documentation>Category=Detector.Camera</xsd:documentation>
      <xsd:documentation>Description=The type of Intensified CCD camera used.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Camera">
          <xsd:attribute name="Type" type="IntensifiedCCDType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The type of intensifiedCCD camera used.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="AnalogVideo" substitutionGroup="CameraGroup">
    <xsd:annotation>
      <xsd:documentation>Category=Detector.Camera</xsd:documentation>
      <xsd:documentation>Description=A device used to capture analog videos.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Camera"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CMOS" substitutionGroup="CameraGroup">
    <xsd:annotation>
      <xsd:documentation>Category=Detector.Camera</xsd:documentation>
      <xsd:documentation>Description=A complementary metal-oxide-semiconductor camera.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Camera">
          <xsd:attribute name="Type" type="CMOSType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The type of CCD camera used.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DetectorWavelengthRange">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>TransmittanceRanges</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This records the range of wavelengths this detector is capable of sensing. Multiple of this elements can be used.
      </xsd:documentation>
      <xsd:documentation>Tier=5</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="CutIn" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=CutIn is the wavelength below which the response rate is below 50%.
          </xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutInUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutOut" use="required" type="PositiveFloat">
        <xsd:annotation>
          <xsd:documentation>Description=CutOut is the wavelength above which the response rate is below 50%.</xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="CutOutUnit" use="optional" default="nm" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="WavelenghtProfile" type="xsd:anyURI">
        <xsd:annotation>
          <xsd:documentation>Description=A file containing the wavelenght profile of this component.</xsd:documentation>
          <xsd:documentation>Tier=5</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <!-- Settings -->
  <xsd:complexType name="Settings">
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><abstract/></xsdfu></xsd:appinfo>
      <xsd:documentation>Settings is an empty complex type that is contained and extended by all the *Settings elements. Each *Settings element defines an attribute named ID of simple type *ID and the other information that is needed. Each simple type *ID is restricted to the base type LSID with an appropriate pattern
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="Reference"> </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="MicroscopeSettings">
    <xsd:annotation>
      <xsd:documentation>Description=This field refers to the Microscope used to acquire this image and holds the setting that were applied to the microscope in this case. The ID is the microscope used in this case.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Settings">
          <xsd:sequence>
            <xsd:element name="OptovarRef">
              <xsd:annotation>
                <xsd:documentation>Description=This empty element has a required reference (the OptovarID attribute) to an existing Optovar component.</xsd:documentation>
                <xsd:documentation>Tier=3</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:complexContent>
                  <xsd:extension base="Reference">
                    <xsd:attribute name="ID" type="OptovarID">
                      <xsd:annotation>
                        <xsd:documentation>Tier=3</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:extension>
                </xsd:complexContent>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="ObjectiveRef">
              <xsd:annotation>
                <xsd:documentation>Description=This empty element has a required reference (the ObjectiveID attribute) to an existing Objective component.</xsd:documentation>
                <xsd:documentation>Tier=3</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:complexContent>
                  <xsd:extension base="Reference">
                    <xsd:attribute name="ID" type="ObjectiveID">
                      <xsd:annotation>
                        <xsd:documentation>Tier=3</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:extension>
                </xsd:complexContent>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="ID" type="MicroscopeID">
            <xsd:annotation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="FieldOfView" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The Field of View defines the area of the specimen that is viewable via the eyepiece. This attribute caputers the measures size of the Field of View.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="FieldOfViewUnit" type="UnitsLength">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="TotalEffectiveMagnification" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This is the total effective magnification used in this case. This number can be obtained by multiplying Optovar/Magnification times the Objective/NominalMagnification or even better the Objective/CalibratedMagnification if available.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="LightSourceSettings">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>LightSourceSettingsCombinations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This holds the setting applied to a LightSource e as well as a reference to the LightSource (the ID attribute) in question. The ID referes to the LightSource used in this case.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Settings">
          <xsd:attribute name="ID" use="required" type="LightSourceID">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Wavelength" use="required" type="PositiveFloat">
            <xsd:annotation>
              <xsd:documentation>Description=The nominal wavelength of the excitation light utilized in this case.
              </xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="WavelengthUnit" use="optional" default="nm" type="UnitsLength">
            <xsd:annotation>
              <xsd:documentation>Description=The units of the Wavelength of the light source - default:nanometres[nm].</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="PowerSettings" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The light power output settings utilized in this case.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="W" name="PowerSettingsUnit" type="UnitsPower">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Attenuation" use="optional" type="PercentFraction">
            <xsd:annotation>
              <xsd:documentation>Description=The observed attenuation of the excitation light power expressed as a fractional value from 0.0 to 1.0.</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ObjectiveSettings">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>ObjectiveSettingsCombinations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This holds the setting applied to an objective as well as a reference to the objective. The ID is the objective used in this case
      </xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Settings">
          <xsd:attribute name="ID" use="required" type="ObjectiveID">
            <xsd:annotation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="CorrectionCollar" use="optional" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The CorrectionCollar is normally an adjustable ring on the objective. Each has an arbitrary scale on it so the values is unit-less.[units:none]</xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ImmersionMedium" type="ObjectiveSettingsImmersionMedium">
            <xsd:annotation>
              <xsd:documentation>Description=The actual immersion medium that was used in this case</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ImmersionRefractiveIndex" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The nominal RefractiveIndex of ther immersion medium utilized in this case. This is a ratio so it also unit-less. [units:none]
              </xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="MeasuredRefractiveIndex" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The temperature dependent measured RefractiveIndex of ther immersion medium utilized in this case. This is a ratio so it also unit-less. [units:none]</xsd:documentation>
              <xsd:documentation>Tier=4</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Temperature" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=The Temperature of the Objective during acquisition.
</xsd:documentation>
              <xsd:documentation>Tier=4</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="TemperatureUnit" use="optional" default="Â°C" type="UnitsTemperature">
            <xsd:annotation>
              <xsd:documentation>Tier=4
          </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DetectorSettingsGroup" abstract="true" type="DetectorSettings">
    <xsd:annotation>
      <xsd:documentation>Description=This element holds the setting applied to a Detector e as well as a reference to the Detector (the ID attribute) in question. The ID referes to the Detector used in this case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="DetectorSettings">
    <xsd:annotation>
      <xsd:documentation>Description=This element holds the setting applied to a Detector e as well as a reference to the Detector (the ID attribute) in question. The ID referes to the Detector used in this case.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="Settings">
        <xsd:attribute name="ID" type="DetectorID" use="required">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="EffectiveOffset" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=This field reports the actual offset utilized for the acquisition of this image. The offset is an electronic baseline added to the output signal from the sensor to ensure that the displayed signal level is always a positive number of counts.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Integration" type="PositiveInt">
          <xsd:annotation>
            <xsd:documentation>Description=This is the number of sequential frames that have been averaged for this image, to improve the signal-to-noise ratio</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CameraFieldOfView" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=The Camera Field of View is the diameter of the field visible by the camera as measured in the specimen plane. It is expressed in mm and it is defined as the Cameara Field Number divided by the Total Effective Magnification of the system. In turn the Field Number is defined as the diameter of the view field in mm measured at the intermediate image plane</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CameraFieldOfViewUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="PhotoMultiplierSettings" substitutionGroup="DetectorSettingsGroup">
    <xsd:annotation>
      <xsd:documentation>Description=This holds the setting applied to a PhotoMultiplier e as well as a reference to the PhotoMultiplier (the ID attribute) in question. The ID referes to the PhotoMultiplier used in this case.</xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="DetectorSettings">
          <xsd:attribute name="PMTGain" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=In general, the gain is a measure of the ability a two-port circuit to add energy to an input signal and therefore increase its power or amplitude. Specifically, PMTGain refers to the ratio of output to input current set for the acquisition of this image</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="PMTVoltage" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This refers to the voltage applied to the photomultiplier for the acquisition of this image</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="V" name="PMTVoltageUnit" type="UnitsElectricPotential">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="EffectiveZoom" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The Zoom factor, aka "Confocal Zoom" or "Scan Zoom", utilized for the acquisition of this image.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CameraSettings" substitutionGroup="DetectorSettingsGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>DetectorSettingsCombinations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This holds the setting applied to a Camera e as well as a reference to the Camera (the ID attribute) in question. The ID referes to the Camera used in this case.
      </xsd:documentation>
      <xsd:documentation>Tier=3</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="DetectorSettings">
          <xsd:attribute name="Binning" use="required" type="Binning">
            <xsd:annotation>
              <xsd:documentation>Description=Represents the number of pixels that are combined to imporve readout rates, signal to noise ratios albeit at the expense of reduced spatial resolution
              </xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="AnalogToDigitalConverter" type="PixelType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=The bit depth used to report the light signal in digital units.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="EMGain" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This is the electron multiplication gain (as apposed to the inherent gain) that was applied to the camera for the acquisition of this image.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ReadOutRate" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=Effective pixel read out rate used for the acquisition of this image. This is the bytes per second that can be read from the detector (like a baud rate).

              </xsd:documentation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ReadOutRateUnit" use="optional" default="Hz" type="UnitsFrequency">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="FrameRate" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=Effective frame rate used for the acquisition of this image.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="fps" name="FrameRateUnit" type="UnitsFrequency"/>
          <xsd:attribute name="OperatingTemperature">
            <xsd:annotation>
              <xsd:documentation>Description=Effective cooling temperature used duringh the acquisition of this image.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="Â°C" name="OperatingTemperatureUnit" type="UnitsTemperature">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- BinaryFile -->
  <xsd:simpleType name="base64Binary">
    <xsd:restriction base="xsd:base64Binary"/>
  </xsd:simpleType>
  <xsd:element name="External">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Externals</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Describes a file location. Can optionally specify a portion of a file using Offset and a ReadLength. If Offset and ReadLength are specified in conjuction with Compression, then they point into the uncompressed file.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="href" use="required" type="xsd:anyURI">
        <xsd:annotation>
          <xsd:documentation>file location</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SHA1" use="required" type="Hex40">
        <xsd:annotation>
          <xsd:documentation>The digest of the file specified in href.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Compression" default="none" use="optional">
        <xsd:annotation>
          <xsd:documentation>Specifies the compression scheme used to encode the data.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="zlib"/>
            <xsd:enumeration value="bzip2"/>
            <xsd:enumeration value="none"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="BinData">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>BinDataBlocks</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=The contents of this element are base64-encoded.  These are not CDATA sections, just a base64 stream.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:extension base="base64Binary">
          <xsd:attribute name="Compression" default="none" use="optional">
            <xsd:annotation>
              <xsd:documentation>Specifies the compression scheme used to encode the data. </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="zlib"/>
                <xsd:enumeration value="bzip2"/>
                <xsd:enumeration value="none"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
          <xsd:attribute name="BigEndian" use="required" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>This is true if the binary data was written in BigEndian order. This is dependent on the system architecture of the machine that wrote the pixels. True for essentially all modern CPUs other than Intel and Alpha. All Binary data must be written in the same endian order.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Length" use="required">
            <xsd:annotation>
              <xsd:documentation>Character count attribute for the BinData field. This is the length of the base-64 encoded block. It allows easy skipping of the block when parsing the file. [unit:bytes]</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="NonNegativeLong"> </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
        </xsd:extension>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="BinaryFile">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>BinaryFiles</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Describes a binary file.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice minOccurs="1" maxOccurs="1">
        <xsd:element ref="External" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="BinData" minOccurs="1" maxOccurs="1"/>
      </xsd:choice>
      <xsd:attribute name="FileName" use="required" type="xsd:string"/>
      <xsd:attribute name="Size" use="required" type="NonNegativeLong">
        <xsd:annotation>
          <xsd:documentation>
            Size of the uncompressed file. [unit:bytes]
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MIMEType" use="optional" type="xsd:string"/>
    </xsd:complexType>
  </xsd:element>

  <!-- SA -->

  <!-- Annotatable objects
       Annotation
       Channel
       Dataset
       Experimenter
       ExperimenterGroup
       Folder
       Image
       Node
       OriginalFile
       Pixels
       PlaneInfo
       Plate
       Project
       Reagent
       Roi
       Screen
       ScreenAcquisition
       Session
       Well
       WellSample

Node ?
OriginalFile ?
Session ?
PlaneInfo (as Plane)
  -->
  <xsd:element name="StructuredAnnotations">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>An unordered collection of annotation attached to objects in the OME data model.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="XMLAnnotation" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="FileAnnotation" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="ListAnnotation" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="LongAnnotation" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="DoubleAnnotation" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="CommentAnnotation" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="BooleanAnnotation" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="TimestampAnnotation" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="TagAnnotation" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="TermAnnotation" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="MapAnnotation" minOccurs="1" maxOccurs="1"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="AnnotationRef">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="AnnotationID">
            <xsd:annotation>
              <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- the base annotation that all others are ultimately derived from -->
  <xsd:complexType name="Annotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Annotations</plural><abstract/></xsdfu></xsd:appinfo>
      <xsd:documentation>An annotation from which all others are ultimately derived.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Description" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Description=This element contains a description for the annotation.[plain-text multi-line string]</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:whiteSpace value="preserve"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:sequence>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:sequence>
    <xsd:attribute name="ID" use="required" type="AnnotationID">
      <xsd:annotation>
        <xsd:documentation>Description=A unique identifier for this component.</xsd:documentation>
        <xsd:documentation>Tier=2</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Namespace" use="required" type="xsd:anyURI">
      <xsd:annotation>
        <xsd:documentation>Description=We recommend the inclusion of a namespace for annotations you define. If it is absent then we assume the annotation is to use our (OME's) default interpretation for this type.</xsd:documentation>
        <xsd:documentation>Tier=5</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Annotator" use="optional" type="ExperimenterID">
      <xsd:annotation>
        <xsd:documentation>Description=The Annotator is the person who attached this annotation.e.g. If UserA annotates something with TagB, owned by UserB, UserA is still the Annotator.</xsd:documentation>
        <xsd:documentation>Tier=2</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:simpleType name="AnnotationID">
    <xsd:restriction base="LSID">
      <xsd:pattern value="(urn:lsid:([\w\-\.]+\.[\w\-\.]+)+:Annotation:\S+)|(Annotation:\S+)"/>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- Intermediate abstract annotations, not used directly -->
  <xsd:complexType name="BasicAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>BasicAnnotations</plural><abstract/></xsdfu></xsd:appinfo>
      <xsd:documentation>An abstract Basic Annotation from which some others are derived.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="Annotation"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="NumericAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>NumericAnnotations</plural><abstract/></xsdfu></xsd:appinfo>
      <xsd:documentation>An abstract Basic Annotation from which some others are derived.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="BasicAnnotation"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TextAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>TextAnnotations</plural><abstract/></xsdfu></xsd:appinfo>
      <xsd:documentation>An abstract Basic Annotation from which some others are derived.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="Annotation"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TypeAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>TypeAnnotations</plural><abstract/></xsdfu></xsd:appinfo>
      <xsd:documentation>An abstract Type Annotation from which some others are derived.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="Annotation"/>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- end of abstract annotations -->

  <!-- The annotations used in the instance documents -->
  <xsd:element name="FileAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>FileAnnotations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>An annotation that consists of a file object.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="TypeAnnotation">
          <xsd:sequence>
            <xsd:element ref="BinaryFile" minOccurs="1" maxOccurs="1"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="XMLAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>XMLAnnotations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>An general xml annotation. The contents of this is not processed as OME XML but should still be well-formed XML.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="TextAnnotation">
          <xsd:sequence>
            <xsd:element name="Value" minOccurs="1" maxOccurs="1">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ListAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>ListAnnotations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>This annotation is a grouping object. It uses the sequence of
annotation refs from the base Annotation to form the list.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Annotation"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CommentAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>CommentAnnotations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=An annotation that consists of a simple multi-line comment.
      </xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="TextAnnotation">
          <xsd:sequence>
            <xsd:element name="Value" type="xsd:string" minOccurs="1" maxOccurs="1">
              <xsd:annotation>
                <xsd:documentation>Description=This element contains the actual content of this annotation. [multi-line string]</xsd:documentation>
                <xsd:documentation>Tier=2</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="LongAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>LongAnnotations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>A simple numerical annotation of type xsd:long.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="NumericAnnotation">
          <xsd:sequence>
            <xsd:element name="Value" type="xsd:long" minOccurs="1" maxOccurs="1"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DoubleAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>DoubleAnnotations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>A simple numerical annotation of type xsd:double.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="NumericAnnotation">
          <xsd:sequence>
            <xsd:element name="Value" type="xsd:double" minOccurs="1" maxOccurs="1"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="BooleanAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>BooleanAnnotations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>A simple boolean annotation of type xsd:boolean.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="BasicAnnotation">
          <xsd:sequence>
            <xsd:element name="Value" type="xsd:boolean" minOccurs="1" maxOccurs="1"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TimestampAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>TimestampAnnotations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>A date/time annotation of type xsd:dateTime.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="BasicAnnotation">
          <xsd:sequence>
            <xsd:element name="Value" type="xsd:dateTime" minOccurs="1" maxOccurs="1"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TagAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>TagAnnotations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        A tag annotation (represents a tag or a tagset)
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="TextAnnotation">
          <xsd:sequence>
            <xsd:element name="Value" type="xsd:string" minOccurs="1" maxOccurs="1"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TermAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>TermAnnotations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>An annotation that consists of an ontology term.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="BasicAnnotation">
          <xsd:sequence>
            <xsd:element name="Value" type="xsd:string" minOccurs="1" maxOccurs="1"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MapAnnotation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>MapAnnotations</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>A map annotation. The contents of this is a list of key/value pairs.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Annotation">
          <xsd:sequence>
            <xsd:element name="Value" type="Map" minOccurs="1" maxOccurs="1"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- ROI elements -->
  <xsd:element name="ShapeGroup" abstract="true" type="Shape"/>
  <!-- top level definition -->
  <xsd:complexType name="Shape">
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Shapes</plural><abstract/></xsdfu></xsd:appinfo>
      <xsd:documentation>
        The shape element contains a single specific ROI shape and links
        that to any channels, and a timepoint and a z-section. It also
        records any transform applied to the ROI shape.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Transform" type="AffineTransform" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>
            This is a matrix used to transform the shape.
            The element has 6 xsd:float attributes. If the element
            is present then all 6 values must be included.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="FillColor" use="optional" type="Color">
      <xsd:annotation>
        <xsd:documentation>
          The color of the fill - encoded as RGBA
          The value "-1" is #FFFFFFFF so solid white (it is a signed 32 bit value)
          NOTE: Prior to the 2012-06 schema the default value was incorrect and produced a transparent red not solid white.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="FillRule" use="optional">
      <xsd:simpleType>
        <xsd:annotation>
          <xsd:documentation>
            The rule used to decide which parts of the shape to
            fill. [enumeration]
          </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="EvenOdd"/>
          <xsd:enumeration value="NonZero"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="StrokeColor" use="optional" type="Color">
      <xsd:annotation>
        <xsd:documentation>
          The color of the stroke  - encoded as RGBA
          The value "-1" is #FFFFFFFF so solid white (it is a signed 32 bit value)
          NOTE: Prior to the 2012-06 schema the default value was incorrect and produced a transparent red not solid white.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="StrokeWidth" use="optional" type="xsd:float">
      <xsd:annotation>
        <xsd:documentation>
          The width of the stroke. Units are set by StrokeWidthUnit.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="StrokeWidthUnit" use="optional" default="pixel" type="UnitsLength">
      <xsd:annotation>
        <xsd:documentation>
          The units used for the stroke width.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="StrokeDashArray" use="optional" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>
          e.g. "none", "10 20 30 10"
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Text" use="optional" type="xsd:string"/>
    <xsd:attribute name="FontFamily" use="optional">
      <xsd:simpleType>
        <xsd:annotation>
          <xsd:documentation>
            The font family used to draw the text. [enumeration]
            Note: these values are all lower case so they match
            the standard HTML/CSS values. "fantasy" has been
            included for completeness; we do not recommend its
            regular use. This attribute is under consideration
            for removal from the OME-XML schema.
          </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="serif"/>
          <xsd:enumeration value="sans-serif"/>
          <xsd:enumeration value="cursive"/>
          <xsd:enumeration value="fantasy"/>
          <xsd:enumeration value="monospace"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="FontSize" use="optional" type="NonNegativeInt">
      <xsd:annotation>
        <xsd:documentation>
          Size of the font. Units are set by FontSizeUnit.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="FontSizeUnit" use="optional" default="pt" type="UnitsLength">
      <xsd:annotation>
        <xsd:documentation>
          The units used for the font size.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="FontStyle" use="optional">
      <xsd:simpleType>
        <xsd:annotation>
          <xsd:documentation>
            The style and weight applied to the text.  [enumeration]
            This is a simplified combination of the HTML/CSS
            attributes font-style AND font-weight.
          </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Bold"/>
          <xsd:enumeration value="BoldItalic"/>
          <xsd:enumeration value="Italic"/>
          <xsd:enumeration value="Normal"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="Locked" use="optional" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>
          Controls whether the shape is locked and read only,
          true is locked, false is editable.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ID" use="required" type="ShapeID"/>
    <xsd:attribute name="TheZ" use="optional" type="NonNegativeInt">
      <xsd:annotation>
        <xsd:documentation>The z-section the ROI applies to. If not specified then the ROI applies to all the z-sections of the image. [units:none] This is numbered from 0.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="TheT" use="optional" type="NonNegativeInt">
      <xsd:annotation>
        <xsd:documentation>
          The timepoint the ROI applies to. If not specified then
          the ROI applies to all the timepoints of the image. [units:none]
          This is numbered from 0.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="TheC" use="optional" type="NonNegativeInt">
      <xsd:annotation>
        <xsd:documentation>
          The channel the ROI applies to. If not specified then
          the ROI applies to all the channels of the image. [units:none]
          This is numbered from 0.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="Rectangle" substitutionGroup="ShapeGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Rectangles</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        A simple rectangle object. If rotation is required apply a
        transformation at the Shape level.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Shape">
          <xsd:attribute name="X" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The X value of the left edge or the rectangle. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Y" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The y value of the top edge or the rectangle. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Width" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The width of the rectangle. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Height" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The height of the rectangle. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Mask" substitutionGroup="ShapeGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Masks</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        The Mask ROI shape is a link to a BinData object that is
        a BIT mask drawn on top of the image as an ROI. It is applied
        at the same scale, pixel to pixel, as the Image the ROI is
        applied to, unless a transform is applied at the shape level.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Shape">
          <xsd:sequence>
            <xsd:element ref="BinData" minOccurs="1" maxOccurs="1"/>
          </xsd:sequence>
          <xsd:attribute name="X" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The X coordinate of the left side of the image. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Y" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The Y coordinate of the top side of the image. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Width" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The width of the mask. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Height" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The height of the mask. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Point" substitutionGroup="ShapeGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Points</plural></xsdfu></xsd:appinfo>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Shape">
          <xsd:attribute name="X" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The X coordinate of the point. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Y" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The Y coordinate of the point. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Ellipse" substitutionGroup="ShapeGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Ellipses</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        A simple ellipse object. If rotation is required apply a
        transformation at the Shape level.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Shape">
          <xsd:attribute name="X" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The X coordinate of the center of the ellipse. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Y" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The Y coordinate of the center of the ellipse. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="RadiusX" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The horizontal radius of the ellipse. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="RadiusY" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>
                The vertical radius of the ellipse. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Line" substitutionGroup="ShapeGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Lines</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        A straight line defined by it's end points.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Shape">
          <xsd:attribute name="X1" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation> The X coordinate of the start of the line. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Y1" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation> The Y coordinate of the start of the line. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="X2" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation> The X coordinate of the end of the line. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Y2" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation> The Y coordinate of the end of the line. [units pixels]
              </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="MarkerStart" use="optional" type="Marker"/>
          <xsd:attribute name="MarkerEnd" use="optional" type="Marker"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Polyline" substitutionGroup="ShapeGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Polylines</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        The Polyline defines open shapes formed of straight
        lines. Note: Polyline uses counterclockwise winding (this is the
        default OpenGL behavior)
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Shape">
          <xsd:attribute name="Points" use="required" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation> The points of the polyline are defined as a list of comma
              separated x,y coordinates seperated by spaces like "x1,y1 x2,y2 x3,y3" e.g.
              "0,0 1,2 3,5" </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="MarkerStart" use="optional" type="Marker"/>
          <xsd:attribute name="MarkerEnd" use="optional" type="Marker"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Polygon" substitutionGroup="ShapeGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Polygons</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        The Polygon defines closed shapes formed of straight
        lines. Note: Polygon uses counterclockwise winding (this is the
        default OpenGL behavior)
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Shape">
          <xsd:attribute name="Points" use="required" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation> The points of the Polygon are defined as a list of comma
              separated x,y coordinates seperated by spaces like "x1,y1 x2,y2 x3,y3" e.g.
              "0,0 1,2 3,5" </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Label" substitutionGroup="ShapeGroup">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Labels</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        The text label. Any transformation should be applied at the
        shape level.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Shape">
          <xsd:attribute name="X" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation> This defines the X coordinate of the current text position
              of the first character in the string. [units pixels] </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Y" use="required" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation> This defines the Y coordinate of the current text position
              of the first character in the string. [units pixels] </xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ROIRef">
    <!-- top level definition -->
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:sequence/>
          <xsd:attribute name="ID" use="required" type="ROIID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="AffineTransform">
    <xsd:annotation>
      <xsd:documentation>
        A matrix used to transform the shape.
        â¡ A00, A01, A02 â¤
        â¢ A10, A11, A12 â¥
        â£ 0,   0,   1   â¦
      </xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="A00" use="required" type="xsd:float"/>
    <xsd:attribute name="A10" use="required" type="xsd:float"/>
    <xsd:attribute name="A01" use="required" type="xsd:float"/>
    <xsd:attribute name="A11" use="required" type="xsd:float"/>
    <xsd:attribute name="A02" use="required" type="xsd:float"/>
    <xsd:attribute name="A12" use="required" type="xsd:float"/>
  </xsd:complexType>
  <xsd:simpleType name="ROIID">
    <!-- top level definition -->
    <xsd:restriction base="LSID">
      <xsd:pattern value="(urn:lsid:([\w\-\.]+\.[\w\-\.]+)+:\S+)|(\S+)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ShapeID">
    <!-- top level definition -->
    <xsd:restriction base="LSID">
      <xsd:pattern value="(urn:lsid:([\w\-\.]+\.[\w\-\.]+)+:Shape:\S+)|(Shape:\S+)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Marker">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Markers</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        Shape of marker on the end of a line. [enumeration]
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Arrow"/>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- Screen Plate Well -->
  <xsd:simpleType name="NamingConvention">
    <xsd:annotation>
      <xsd:documentation>
        Predefined list of values for the well labels
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="letter">
        <xsd:annotation>
          <xsd:documentation>
            While the label type 'number' has a clear meaning the 'letter'
            type is more complex.

            If you have less than 26 values use letters A to Z.
            Once you get more than 26 values there are several different
            approaches in use.
            One we have see include:

            Single letter, then double letter each running A to Z, right first
            e.g. A, B, C, ... X, Y, Z, AA, AB, AC, ... AY, AZ, BA, BB, ...
            This is the format used by Microsoft Excel so users may be familiar with it.
            This is the approach we use in the OMERO client applications.

            CAPITALsmall, each running A to Z, small first
            e.g. Aa, Ab, Ac, ... Ax, Ay, Az, Ba, Bb, Bc, ... By, Bz, Ca, Cb, ...
            This is in use by some plate manufactures.

            Single letter, then double letter, then triple letter, and so on
            e.g. A, B, C, ... X, Y, Z, AA, BB, CC, ... YY, ZZ, AAA, BBB, ...
            This has the advantage that the first 26 are the same as the standard
            but has a problem an the labels get wider and wider leading to user
            interface problems.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="number">
        <xsd:annotation>
          <xsd:documentation>
            1, 2, 3, ...
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PlateID">
    <xsd:restriction base="LSID">
      <xsd:pattern value="(urn:lsid:([\w\-\.]+\.[\w\-\.]+)+:Plate:\S+)|(Plate:\S+)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="Reagent">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Reagents</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        Reagent is used to describe a chemical or some other physical experimental parameter.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>
              A long description for the reagent.
            </xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="ReagentID"/>
      <xsd:attribute name="Name" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            A short name for the reagent.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ReagentIdentifier" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            This is a reference to an external (to OME) representation of the Reagent.
            It serves as a foreign key into an external database. - It is sometimes referred to as ExternalIdentifier.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:simpleType name="ReagentID">
    <xsd:restriction base="LSID">
      <xsd:pattern value="(urn:lsid:([\w\-\.]+\.[\w\-\.]+)+:Reagent:\S+)|(Reagent:\S+)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ScreenID">
    <xsd:restriction base="LSID">
      <xsd:pattern value="(urn:lsid:([\w\-\.]+\.[\w\-\.]+)+:Screen:\S+)|(Screen:\S+)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="PlateAcquisition">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>PlateAcquisitions</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        PlateAcquisition is used to describe a single acquisition run for a plate.
        This object is used to record the set of images acquired in a single
        acquisition run. The Images for this run are linked to PlateAcquisition
        through WellSample.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>
              A description for the PlateAcquisition.
            </xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element ref="WellSampleRef" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="PlateAcquisitionID"/>
      <xsd:attribute name="Name" use="optional" type="xsd:string"/>
      <xsd:attribute name="EndTime" use="optional" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>
            Time when the last image of this acquisition was collected
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="StartTime" use="optional" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>
            Time when the first image of this acquisition was collected
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="MaximumFieldCount" use="optional" type="PositiveInt">
        <xsd:annotation>
          <xsd:documentation>
            The maximum number of fields (well samples) in any well
            in this PlateAcquisition.
            This is only used to speed up user interaction by stopping
            the reading of every well sample.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:simpleType name="PlateAcquisitionID">
    <xsd:restriction base="LSID">
      <xsd:pattern
        value="(urn:lsid:([\w\-\.]+\.[\w\-\.]+)+:PlateAcquisition:\S+)|(PlateAcquisition:\S+)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="Well">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Wells</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        A Well is a component of the Well/Plate/Screen construct to describe screening applications.
        A Well has a number of WellSample elements that link to the Images collected in this well.
        The ReagentRef links any Reagents that were used in this Well. A well is part of only one Plate.
        The origin for the row and column identifiers is the top left corner of the plate starting at zero.
        i.e The top left well of a plate is index (0,0)
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="WellSample" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><ordered/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="ReagentRef" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="AnnotationRef" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><manytomany/></xsdfu></xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="WellID"/>
      <xsd:attribute name="Column" use="required" type="NonNegativeInt">
        <xsd:annotation>
          <xsd:documentation>
            This is the column index of the well, the origin is the top left corner of the plate
            with the first column of cells being column zero. i.e top left is (0,0)
            The combination of Row, Column has to be unique for each well in a plate.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Row" use="required" type="NonNegativeInt">
        <xsd:annotation>
          <xsd:documentation>
            This is the row index of the well, the origin is the top left corner of the plate
            with the first row of wells being row zero. i.e top left is (0,0)
            The combination of Row, Column has to be unique for each well in a plate.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ExternalDescription" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            A description of the externally defined identifier for this plate.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="ExternalIdentifier" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The ExternalIdentifier attribute may contain a reference to an external database.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Type" use="optional" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>A human readable identifier for the screening status. e.g. empty, positive control, negative control, control, experimental, etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Color" use="optional" type="Color" default="-1">
        <xsd:annotation>
          <xsd:documentation>A marker color used to highlight the well - encoded as RGBA The default value "-1" is #FFFFFFFF so solid white (it is a signed 32 bit value) NOTE: Prior to the 2012-06 schema the default value was incorrect and produced a transparent red not solid white.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:simpleType name="WellID">
    <xsd:restriction base="LSID">
      <xsd:pattern value="(urn:lsid:([\w\-\.]+\.[\w\-\.]+)+:Well:\S+)|(Well:\S+)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="WellSample">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>WellSamples</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>
        WellSample is an individual image that has been captured within a Well.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ImageRef" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:appinfo><xsdfu xmlns=""><injected/></xsdfu></xsd:appinfo>
            <xsd:documentation>
              This is the main link to the core Image element
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="WellSampleID"/>
      <xsd:attribute name="PositionX" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>
            The X position of the field (image) within the well relative to
            the well origin defined on the Plate. Units are set by PositionXUnit.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PositionXUnit" use="optional" default="reference frame"
        type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>The units of the position in X - default:reference frame.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PositionY" use="optional" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>
            The Y position of the field (image) within the well relative to
            the well origin defined on the Plate. Units are set by PositionYUnit.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="PositionYUnit" use="optional" default="reference frame"
        type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>The units of the position in Y - default:reference frame.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Timepoint" use="optional" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>
            The time-point at which the image started to be collected
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Index" use="required" type="NonNegativeInt">
        <xsd:annotation>
          <xsd:documentation>
            This records the order of the well samples. Each
            index should be unique for a given plate but they do not
            have to be sequential, there may be gaps if part of the
            dataset is missing. In the user interface the displayed
            value of the index will be calculated modulo the number
            of PlateAcquisitions for the plate.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:simpleType name="WellSampleID">
    <xsd:restriction base="LSID">
      <xsd:pattern value="(urn:lsid:([\w\-\.]+\.[\w\-\.]+)+:WellSample:\S+)|(WellSample:\S+)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="WellSampleRef">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>
        The WellSampleRef element is a reference to a WellSample element.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="WellSampleID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- References -->
  <xsd:complexType name="Reference">
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><abstract/></xsdfu></xsd:appinfo>
      <xsd:documentation>
        Reference is an empty complex type that is contained and extended by all the *Ref elements and also the Settings Complex Type
        Each *Ref element defines an attribute named ID of simple type *ID and no other information
        Each simple type *ID is restricted to the base type LSID with an appropriate pattern
      </xsd:documentation>
    </xsd:annotation>
  </xsd:complexType>
  <xsd:element name="ImageRef">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>
        The ImageRef element is a reference to an Image element.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="ImageID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TIRFSettingsRef">
    <xsd:annotation>
      <xsd:documentation>This empty element has a reference (the TIRFSettingsID attribute) to a TIRF Settings defined into OME. E.g. a microbeam operation, such as a laser used for photobleaching.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" type="TIRFSettingsID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MicrobeamManipulationRef">
    <!-- top level definition -->
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="MicrobeamManipulationID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ExperimentRef">
    <!-- top level definition -->
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="ExperimentID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ChannelRef">
    <!-- top level definition -->
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="ChannelID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ProjectRef">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>
        There may be one or more of these in a Dataset.
        This empty element has a required Project ID attribute that refers to Projects defined within the OME element.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="ProjectID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ExperimenterRef">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>Description=This empty element has a required reference (the ExperimenterID attribute) to a Experimenter defined within OME
      </xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="ExperimenterID">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ExperimenterGroupRef">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>Description=This empty element has a reference (the ExperimenterGroup ID attribute) to a ExperimenterGroup defined within OME
</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="ExperimenterGroupID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="InstrumentRef">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>Description=This empty element can be used (via the required Instrument ID attribute) to refer to an Instrument defined within OME</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="InstrumentID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DatasetRef">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>The DatasetRef element refers to a Dataset by specifying the Dataset ID attribute. One or more DatasetRef elements may be listed within the Image element to specify what Datasetsthe Image belongs to.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="DatasetID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FolderRef">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:documentation>The FolderRef element refers to a Folder by specifying the Folder ID attribute. One or more FolderRef elements may be listed within the Folder element to specify what Folders the Folder contains. This tree hierarchy must be acyclic.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="FolderID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FilterSetRef">
    <!-- top level definition -->
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="FilterSetID">
            <xsd:annotation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ReagentRef">
    <!-- top level definition -->
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" use="required" type="ReagentID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="LensRef">
    <xsd:complexContent>
      <xsd:extension base="Reference">
        <xsd:attribute name="ID" type="LensID"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="PrismRef">
    <xsd:complexContent>
      <xsd:extension base="Reference">
        <xsd:attribute name="ID" type="PrismID"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="PolarizationOpticsRef">
    <xsd:complexContent>
      <xsd:extension base="Reference">
        <xsd:attribute name="ID" type="PolarizationOpticsID"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="FilterRef">
    <!-- top level definition -->
    <xsd:complexContent>
      <xsd:extension base="Reference">
        <xsd:attribute name="ID" use="required" type="FilterID">
          <xsd:annotation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MirrorRef">
    <xsd:complexContent>
      <xsd:extension base="Reference">
        <xsd:attribute name="ID" type="MirrorID">
          <xsd:annotation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="StandardDichroicRef" type="MirrorRef">
    <!-- top level definition --> </xsd:element>
  <xsd:element name="LightSourceCouplingRef">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" type="LightSourceCouplingID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SampleHolderRef">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" type="SampleHolderID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SamplePositioningRef">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" type="SamplePositioningID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="AutoFocusRef">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" type="AutoFocusID"/>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Miscellaneous complex types -->
  <xsd:element name="TransmittanceProfileRef">
    <xsd:annotation>
      <xsd:documentation>Description=This empty element has a reference (the FileID attribute) to a file in .csv or other suitable formats containing the wavelenght profile of this component.</xsd:documentation>
      <xsd:documentation>Tier=5</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Reference">
          <xsd:attribute name="ID" type="TransmittanceProfileID">
            <xsd:annotation>
              <xsd:documentation>Tier=5</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="ManufacturerSpec">
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><abstract/></xsdfu></xsd:appinfo>
      <xsd:documentation>Description=This describes the manufacturer specifications for this component (i.e., manufacturer name, model designation and serial number).</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="Manufacturer" use="required" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Description=The manufacturer of this component.</xsd:documentation>
        <xsd:documentation>Tier=1</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Model" use="required" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Description=The model of this component.</xsd:documentation>
        <xsd:documentation>Tier=3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="SerialNumber" use="required" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Description=The serial number of this component</xsd:documentation>
        <xsd:documentation>Tier=5</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="LotNumber" use="optional" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Description=The lot number of this component</xsd:documentation>
        <xsd:documentation>Tier=5</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="SpecsFile" use="required" type="xsd:anyURI">
      <xsd:annotation>
        <xsd:documentation>Description=Provide the location of a PDF file with the specifications for the component as provided by the manufacturer.</xsd:documentation>
        <xsd:documentation>Tier=5</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Map">
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns=""><plural>Maps</plural></xsdfu></xsd:appinfo>
      <xsd:documentation>This is a Mapping of key/value pairs.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence minOccurs="1" maxOccurs="1">
      <xsd:element name="M" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:appinfo><xsdfu xmlns=""><plural>Ms</plural></xsdfu></xsd:appinfo>
          <xsd:documentation>Description=This is a key/value pair used to build up a Mapping. The Element and Attribute name are kept to single letters to minimize the length at the expense of readability as they are likely to occur many times.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:string">
              <xsd:attribute name="K" type="xsd:string"/>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!-- Miscellaneus simple types -->
  <xsd:simpleType name="Denomination">
    <xsd:annotation>
      <xsd:documentation>A user defined designation [i.e. name] for this element.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="Hex40">
    <xsd:annotation>
      <xsd:documentation>
        Binary contents coded in hexadecimal (20 characters long)
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:hexBinary">
      <xsd:length value="20"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NonNegativeFloat">
    <xsd:annotation>
      <xsd:documentation>
        A simple type that restricts the value to a float between >=0 and max 32-bit float {i.e. (2â2^-23) Ã 2^27 â 3.4 Ã 10^38}
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:float">
      <xsd:minInclusive value="0"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NonNegativeLong">
    <xsd:annotation>
      <xsd:documentation>
        A simple type that restricts the value to a long between 0 and 9223372036854775807 (inclusive).
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:long">
      <xsd:minInclusive value="0"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NonNegativeInt">
    <xsd:annotation>
      <xsd:documentation>
        A simple type that restricts the value to an integer between 0 and 2,147,483,647 (inclusive).
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PositiveInt">
    <xsd:annotation>
      <xsd:documentation>
        A simple type that restricts the value to an integer between 1 and 2,147,483,647 (inclusive).
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="1"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PositiveFloat">
    <xsd:annotation>
      <xsd:documentation>
        A simple type that restricts the value to a float between >0 and max 32-bit float {i.e. (2â2^-23) Ã 2^27 â 3.4 Ã 10^38}
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:float">
      <xsd:minExclusive value="0.0"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PercentFraction">
    <xsd:annotation>
      <xsd:documentation>
        A simple type that restricts the value to a float between 0 and 1 (inclusive).
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:float">
      <xsd:maxInclusive value="1.0"/>
      <xsd:minInclusive value="0.0"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="UniversallyUniqueIdentifier">
    <xsd:annotation>
      <xsd:documentation>
        This is a unique ID for the file but does not conform to the ID pattern used in the rest of the file.
        The rest of the IDs are either an full LSID or an internal ID which is a string that is simply unique in this file.
        As the UniversallyUniqueIdentifier is used from outside this file to identify it having the same ID in another file could cause problems.
        A UUID is 32 hexadecimal digits, in 5 groups, 8-4-4-4-12, separated by hyphens
        e.g. urn:uuid:3e450fae-b8f2-4d35-aa54-702168b2487f
        There are methods to generate these in most modern languages.
        http://www.ietf.org/rfc/rfc4122.txt
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:anyURI">
      <xsd:pattern
        value="(urn:uuid:[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12})"
      />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Binning">
    <xsd:annotation>
      <xsd:documentation>
        Represents the number of pixels that are combined to form larger pixels. {used:CCD,EMCCD}
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="1x1">
        <xsd:annotation>
          <xsd:documentation>No binning.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns="">
            <enum name="1Ã1" enum="ONEBYONE" compatregex="^\\s*1.*x.*1\\s*"/>
          </xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2x2">
        <xsd:annotation>
          <xsd:documentation>2Ã2 binning.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns="">
            <enum name="2Ã2" enum="TWOBYTWO" compatregex="^\\s*2.*x.*2\\s*"/>
          </xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="4x4">
        <xsd:annotation>
          <xsd:documentation>4Ã4 binning.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns="">
            <enum name="4Ã4" enum="FOURBYFOUR" compatregex="^\\s*4.*x.*4\\s*"/>
          </xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="8x8">
        <xsd:annotation>
          <xsd:documentation>8Ã8 binning.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns="">
            <enum name="8Ã8" enum="EIGHTBYEIGHT" compatregex="^\\s*8.*x.*8\\s*"/>
          </xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Other">
        <xsd:annotation>
          <xsd:documentation>Other binning value.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns="">
            <enum enum="OTHER"/>
          </xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="FontFamily">
    <xsd:annotation>
      <xsd:documentation>
        The font family used to draw the text. [enumeration]
        Note: these values are all lower case so they match
        the standard HTML/CSS values. "fantasy" has been
        included for completeness we do not recommended its
        regular use.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="serif">
        <xsd:annotation>
          <xsd:documentation>serif.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns="">
            <enum enum="SERIF"/>
          </xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="sans-serif">
        <xsd:annotation>
          <xsd:documentation>sans-serif.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns="">
            <enum enum="SANSSERIF"/>
          </xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="cursive">
        <xsd:annotation>
          <xsd:documentation>cursive.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns="">
            <enum enum="CURSIVE"/>
          </xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="fantasy">
        <xsd:annotation>
          <xsd:documentation>fantasy.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns="">
            <enum enum="FANTASY"/>
          </xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="monospace">
        <xsd:annotation>
          <xsd:documentation>monospace.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns="">
            <enum enum="MONOSPACE"/>
          </xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PixelType">
    <xsd:annotation>
      <xsd:documentation>
        The number size/kind used to represent a pixel
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="int8">
        <xsd:annotation>
          <xsd:documentation>8 bit signed integer.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns=""><enum enum="INT8"/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="int16">
        <xsd:annotation>
          <xsd:documentation>16 bit signed integer.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns=""><enum enum="INT16"/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="int32">
        <xsd:annotation>
          <xsd:documentation>32 bit signed integer.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns=""><enum enum="INT32"/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="uint8">
        <xsd:annotation>
          <xsd:documentation>8 bit unsigned integer.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns=""><enum enum="UINT8"/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="uint16">
        <xsd:annotation>
          <xsd:documentation>16 bit unsigned integer.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns=""><enum enum="UINT16"/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="uint32">
        <xsd:annotation>
          <xsd:documentation>32 bit unsigned integer.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns=""><enum enum="UINT32"/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="float">
        <xsd:annotation>
          <xsd:documentation>single-precision floating point.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns=""><enum enum="FLOAT"/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="double">
        <xsd:annotation>
          <xsd:documentation>double-precision floating point.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns=""><enum enum="DOUBLE"/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="complex">
        <xsd:annotation>
          <xsd:documentation>complex single-precision floating point.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns=""><enum enum="COMPLEXFLOAT"/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="double-complex">
        <xsd:annotation>
          <xsd:documentation>complex double-precision floating point.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns=""><enum enum="COMPLEXDOUBLE"/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="bit">
        <xsd:annotation>
          <xsd:documentation>bit mask.</xsd:documentation>
          <xsd:appinfo><xsdfu xmlns=""><enum enum="BIT"/></xsdfu></xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Color">
    <xsd:annotation>
      <xsd:documentation>
        A simple type that identifies itself as a Color, the value is an integer between -2,147,483,648 and 2,147,483,647 (inclusive).
        The value is a signed 32 bit encoding of RGBA so "-1" is #FFFFFFFF or solid white.
        NOTE: Prior to the 2012-06 schema the default values were incorrect and produced a transparent red not solid white.
      </xsd:documentation>
    </xsd:annotation>
